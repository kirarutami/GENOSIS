<!-- Events Class -->
    <owl:Class rdf:about="#OSN:Events">
        <rdfs:subClassOf rdf:resource="#OSN:AdditionalFeature"/>
        <rdfs:comment>Represents events created and managed by users.</rdfs:comment>
    </owl:Class>

    <!-- Data Properties for Events -->

    <owl:DatatypeProperty rdf:about="#OSN:hasEventDetails">
        <rdfs:domain rdf:resource="#OSN:Events"/>
        <rdfs:label xml:lang="en">has Events Details</rdfs:label>
        <rdfs:comment xml:lang="en">A generic property for events details</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasEventCreationTimestamp">
        <rdfs:subPropertyOf rdf:resource="#OSN:hasEventDetails"/>
        <rdfs:domain rdf:resource="#OSN:Events"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The timestamp when the event was created.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasEventUpdateTimestamp">
        <rdfs:subPropertyOf rdf:resource="#OSN:hasEventDetails"/>
        <rdfs:domain rdf:resource="#OSN:Events"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The timestamp when the event was last updated.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasEventStartTimestamp">
          <rdfs:subPropertyOf rdf:resource="#OSN:hasEventDetails"/>
        <rdfs:domain rdf:resource="#OSN:Events"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The timestamp when the event starts.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasEventEndTimestamp">
        <rdfs:subPropertyOf rdf:resource="#OSN:hasEventDetails"/>
        <rdfs:domain rdf:resource="#OSN:Events"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The timestamp when the event ends.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasEventInteractionIP">
        <rdfs:subPropertyOf rdf:resource="#OSN:hasEventDetails"/>
        <rdfs:domain rdf:resource="#OSN:Events"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The IP address of the user managing the event.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasEventDeviceInfo">
        <rdfs:subPropertyOf rdf:resource="#OSN:hasEventDetails"/>
        <rdfs:domain rdf:resource="#OSN:Events"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Device information of the user managing the event.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- EventInvitations Sub-class -->
    <owl:Class rdf:about="#EventInvitations">
        <rdfs:subClassOf rdf:resource="#OSN:Events"/>
        <rdfs:comment>Handles invitations for events.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for EventInvitations -->
    <owl:ObjectProperty rdf:about="#sendsEventInvitationTo">
        <rdfs:domain rdf:resource="#OSN:User"/>
        <rdfs:range rdf:resource="#OSN:User"/>
        <rdfs:comment>Represents a user sending an event invitation to another user.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#acceptsEventInvitationFrom">
        <rdfs:domain rdf:resource="#OSN:User"/>
        <rdfs:range rdf:resource="#OSN:User"/>
        <rdfs:comment>Represents a user accepting an event invitation from another user.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    </owl:ObjectProperty>

    <!-- Data Properties for EventInvitations -->
    <owl:DatatypeProperty rdf:about="#hasInvitationSentTimestamp">
        <rdfs:domain rdf:resource="#EventInvitations"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The timestamp when the invitation was sent.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasInviteAcceptanceTimestamp">
        <rdfs:domain rdf:resource="#EventInvitations"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The timestamp when the invitation was accepted.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- EventTypes Sub-class -->
    <owl:Class rdf:about="#EventTypes">
        <rdfs:subClassOf rdf:resource="#OSN:Events"/>
        <rdfs:comment>Represents different types of events.</rdfs:comment>
    </owl:Class>

    <!-- Individuals for EventTypes -->
    <owl:NamedIndividual rdf:about="#InPerson">
        <rdf:type rdf:resource="#EventTypes"/>
        <rdfs:comment>Represents an in-person event type.</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Virtual">
        <rdf:type rdf:resource="#EventTypes"/>
        <rdfs:comment>Represents a virtual event type.</rdfs:comment>
    </owl:NamedIndividual>

