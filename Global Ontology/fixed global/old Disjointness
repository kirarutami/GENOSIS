

<!--Disjointness-->
    <owl:Class rdf:about="#Global:UserAccount">
        <rdfs:comment xml:lang="en">An instance of Global:UserAccount cannot simultaneously be an instance of Global:Content, Global:Message, Global:ContentInteraction, Global:Group, Global:SearchFunctionality, Global:Marketplace, or Global:AdditionalFeature, as these represent fundamentally different top-level categories.</rdfs:comment>
        <owl:disjointWith rdf:resource="#Global:Content"/>
        <owl:disjointWith rdf:resource="#Global:Message"/>
        <owl:disjointWith rdf:resource="#Global:ContentInteraction"/>
        <!-- <owl:disjointWith rdf:resource="#Global:Group"/> -->
        <owl:disjointWith rdf:resource="#Global:SearchFunctionality"/>
        <owl:disjointWith rdf:resource="#Global:Marketplace"/>
        <!-- <owl:disjointWith rdf:resource="#Global:AdditionalFeature"/> -->
    </owl:Class>

    <owl:Class rdf:about="#Global:Content">
