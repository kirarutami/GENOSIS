    <owl:Ontology rdf:about="http://example.org/gsmfo.owl">
        <dc:title xml:lang="en">GSMFO - Global Social Media Forensic Ontology (YouTube Case)</dc:title>
        <owl:DatatypeProperty rdf:about="#belongsToPlatform">
            <rdfs:label xml:lang="en">belongs to Platform</rdfs:label>
            <rdfs:comment xml:lang="en">Indicates the social media platform to which an entity (e.g., user account, post, message) belongs.</rdfs:comment>
            <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>

        <owl:DatatypeProperty rdf:about="#hasUserID">
            <rdfs:label xml:lang="en">has User ID</rdfs:label>
            <rdfs:comment xml:lang="en">A unique identifier for the user on the platform.</rdfs:comment>
            <rdfs:subPropertyOf rdf:resource="#Global:hasProfileDetails"/>
            <rdfs:domain rdf:resource="#Global:UserAccount"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasPasswordHash">
            <rdfs:label xml:lang="en">has Password Hash</rdfs:label>
            <rdfs:comment xml:lang="en">Stores the hashed password of the user.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:UserAccount"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasPremiumMembershipStatus">
            <rdfs:label xml:lang="en">has Premium Membership Status</rdfs:label>
            <rdfs:comment xml:lang="en">Indicates if the user has a premium membership.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:UserAccount"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        </owl:DatatypeProperty>

        <owl:DatatypeProperty rdf:about="#hasVideoID">
            <rdfs:label xml:lang="en">has Video ID</rdfs:label>
            <rdfs:comment xml:lang="en">A unique identifier for the video.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:VideoContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasTitle"> <rdfs:label xml:lang="en">has Title</rdfs:label>
            <rdfs:comment xml:lang="en">The title of a content item like a video or post.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:Content"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasVideoDescription"> <rdfs:label xml:lang="en">has Video Description</rdfs:label>
            <rdfs:comment xml:lang="en">The description of a video.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:VideoContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasPublicationDate">
            <rdfs:label xml:lang="en">has Publication Date</rdfs:label>
            <rdfs:comment xml:lang="en">The date when a content item was published.</rdfs:comment>
            <rdfs:subPropertyOf rdf:resource="#Global:hasCreationTimestamp"/>
            <rdfs:domain rdf:resource="#Global:Content"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasDurationInMinutes">
            <rdfs:label xml:lang="en">has Duration In Minutes</rdfs:label>
            <rdfs:comment xml:lang="en">The duration of a video or audio content in minutes.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:VideoContent"/> <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasDislikeCount">
            <rdfs:label xml:lang="en">has Dislike Count</rdfs:label>
            <rdfs:comment xml:lang="en">The number of dislikes a content item has received.</rdfs:comment>
            <rdfs:subPropertyOf rdf:resource="#Global:count"/>
            <rdfs:domain rdf:resource="#Global:Content"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasVideoTags">
            <rdfs:label xml:lang="en">has Video Tags</rdfs:label>
            <rdfs:comment xml:lang="en">Tags associated with a video.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:VideoContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasContentRating">
            <rdfs:label xml:lang="en">has Content Rating</rdfs:label>
            <rdfs:comment xml:lang="en">The content rating (e.g., G, PG, Mature).</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:Content"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>

        <owl:DatatypeProperty rdf:about="#hasLiveStartTime">
            <rdfs:label xml:lang="en">has Live Start Time</rdfs:label>
            <rdfs:comment xml:lang="en">The start time of a live event.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:LiveContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasLiveEndTime">
            <rdfs:label xml:lang="en">has Live End Time</rdfs:label>
            <rdfs:comment xml:lang="en">The end time of a live event.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:LiveContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasLiveDurationInMinutes">
            <rdfs:label xml:lang="en">has Live Duration In Minutes</rdfs:label>
            <rdfs:comment xml:lang="en">The duration of a live event in minutes.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:LiveContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasViewerCountInLive">
            <rdfs:label xml:lang="en">has Viewer Count In Live</rdfs:label>
            <rdfs:comment xml:lang="en">The number of viewers during a live event.</rdfs:comment>
            <rdfs:subPropertyOf rdf:resource="#Global:viewCount"/>
            <rdfs:domain rdf:resource="#Global:LiveContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasReactionCountInLive">
            <rdfs:label xml:lang="en">has Reaction Count In Live</rdfs:label>
            <rdfs:comment xml:lang="en">The number of reactions during a live event.</rdfs:comment>
            <rdfs:subPropertyOf rdf:resource="#Global:count"/>
            <rdfs:domain rdf:resource="#Global:LiveContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasLiveTags">
            <rdfs:label xml:lang="en">has Live Tags</rdfs:label>
            <rdfs:comment xml:lang="en">Tags associated with a live event.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:LiveContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasSuperChatRevenue">
            <rdfs:label xml:lang="en">has SuperChat Revenue</rdfs:label>
            <rdfs:comment xml:lang="en">Revenue generated from SuperChats during a live event.</rdfs:comment>
            <rdfs:domain rdf:resource="#Global:LiveContent"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        </owl:DatatypeProperty>

        <owl:DatatypeProperty rdf:about="#hasDonationAmount"> <rdfs:label xml:lang="en">has Donation Amount</rdfs:label>
            <rdfs:comment xml:lang="en">The amount of a donation or SuperChat.</rdfs:comment>
            <rdfs:domain rdf:resource="#MCSS:SuperChat"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        </owl:DatatypeProperty>
        <owl:DatatypeProperty rdf:about="#hasDonationTimestamp"> <rdfs:label xml:lang="en">has Donation Timestamp</rdfs:label>
            <rdfs:comment xml:lang="en">The timestamp of a donation or SuperChat.</rdfs:comment>
            <rdfs:subPropertyOf rdf:resource="#Global:hasInteractionTimestamp"/>
            <rdfs:domain rdf:resource="#MCSS:SuperChat"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        </owl:DatatypeProperty>
        <owl:ObjectProperty rdf:about="#donatedBy"> <rdfs:label xml:lang="en">donated By</rdfs:label>
            <rdfs:comment xml:lang="en">Relates a donation or SuperChat to the user who made it.</rdfs:comment>
            <rdfs:domain rdf:resource="#MCSS:SuperChat"/>
            <rdfs:range rdf:resource="#Global:UserAccount"/>
        </owl:ObjectProperty>
