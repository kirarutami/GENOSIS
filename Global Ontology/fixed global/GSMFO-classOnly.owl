<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/gsmfo.owl#"
     xml:base="http://example.org/gsmfo.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:OSN="http://saralutami.org/OnlineSocialNetworkSites#"
     xmlns:MP="http://saralutami.org/MicrobloggingPlatforms#"
     xmlns:MCSS="http://saralutami.org/MediaContentSharingSites#"
     xmlns:OFB="http://saralutami.org/OnlineForumsBlogs#"
     xmlns:GSMFO="http://saralutami.org/GSMFO#">

    <owl:Ontology rdf:about="http://example.org/gsmfo.owl">
        <dc:title xml:lang="en">GSMFO - Global Social Media Forensic Ontology (Integrated Classes V2)</dc:title>
        <dc:description xml:lang="en">This ontology integrates classes from four local ontologies (OSN, MP, MCSS, OFB) based on a defined global hierarchy, focusing on generalization for social media forensics.</dc:description>
        <dc:description xml:lang="id">Ontologi ini mengintegrasikan kelas-kelas dari empat ontologi lokal (OSN, MP, MCSS, OFB) berdasarkan hierarki global yang telah ditentukan, dengan fokus pada generalisasi untuk forensik media sosial.</dc:description>
        <owl:versionInfo>2.0</owl:versionInfo>
    </owl:Ontology>

    <owl:AnnotationProperty rdf:about="#OSNcomment">
        <rdfs:label xml:lang="en">OSN Comment</rdfs:label>
        <rdfs:comment xml:lang="en">Original comment from the OSN local ontology.</rdfs:comment>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="#MPcomment">
        <rdfs:label xml:lang="en">MP Comment</rdfs:label>
        <rdfs:comment xml:lang="en">Original comment from the MP local ontology.</rdfs:comment>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="#MCSScomment">
        <rdfs:label xml:lang="en">MCSS Comment</rdfs:label>
        <rdfs:comment xml:lang="en">Original comment from the MCSS local ontology.</rdfs:comment>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="#OFBcomment">
        <rdfs:label xml:lang="en">OFB Comment</rdfs:label>
        <rdfs:comment xml:lang="en">Original comment from the OFB local ontology.</rdfs:comment>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="#alignWithOSN">
      <rdfs:label xml:lang="en">Aligned with OSN</rdfs:label>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="#alignWithMP">
      <rdfs:label xml:lang="en">Aligned with MP</rdfs:label>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="#alignWithMCSS">
      <rdfs:label xml:lang="en">Aligned with MCSS</rdfs:label>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="#alignWithOFB">
      <rdfs:label xml:lang="en">Aligned with OFB</rdfs:label>
    </owl:AnnotationProperty>
     <owl:AnnotationProperty rdf:about="#sourceOrigin">
      <rdfs:label xml:lang="en">Source Origin</rdfs:label>
      <rdfs:comment xml:lang="en">Indicates the original source or mapping information for this element from the merging process.</rdfs:comment>
    </owl:AnnotationProperty>

<!--UserAccount-->
    <owl:Class rdf:about="#Global:UserAccount">
        <rdfs:label xml:lang="en">Global User Account</rdfs:label>
        <rdfs:comment xml:lang="en">Represents a generalized user account across different social media platforms. This is a core entity for forensic analysis.</rdfs:comment>
        <rdfs:comment xml:lang="id">Merepresentasikan akun pengguna yang digeneralisasi lintas platform media sosial. Ini adalah entitas inti untuk analisis forensik.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#MCSS:User">
        <rdfs:subClassOf rdf:resource="#Global:UserAccount"/>
        <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The primary class representing individual users in the system.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#User</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#MP:User">
        <rdfs:subClassOf rdf:resource="#Global:UserAccount"/>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a user in a microblogging platform (e.g., Twitter, Tumblr).</MPcomment>
        <alignWithMP rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MicrobloggingPlatforms#User</alignWithMP>
    </owl:Class>
    <owl:Class rdf:about="#MP:List">
        <rdfs:subClassOf rdf:resource="#MP:User"/>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents lists created or followed by users on the platform.</MPcomment>
    </owl:Class>
    <owl:Class rdf:about="#MP:Bookmark">
        <rdfs:subClassOf rdf:resource="#MP:User"/>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a collection of content items saved as bookmarks by users.</MPcomment>
    </owl:Class>
    <owl:Class rdf:about="#OFB:User">
        <rdfs:subClassOf rdf:resource="#Global:UserAccount"/>
        <OFBcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a user in the online forum or blog system.</OFBcomment>
        <alignWithOFB rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineForumBlogs#User</alignWithOFB>
    </owl:Class>
    <owl:Class rdf:about="#OSN:User">
        <rdfs:subClassOf rdf:resource="#Global:UserAccount"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The primary class representing the user in the system.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#User</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#OSN:CloseFriendList">
        <rdfs:subClassOf rdf:resource="#OSN:User"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a list of close friends who can view exclusive stories from the user.</OSNcomment>
    </owl:Class>

<!--Content-->
    <owl:Class rdf:about="#Global:Content">
        <rdfs:label xml:lang="en">Global Content</rdfs:label>
        <rdfs:comment xml:lang="en">A generalized representation of any content created or shared on social media platforms. This is a superclass for specific content types.</rdfs:comment>
        <rdfs:comment xml:lang="id">Representasi umum dari konten apapun yang dibuat atau dibagikan di platform media sosial. Ini adalah superclass untuk tipe konten spesifik.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Global:Post">
        <rdfs:subClassOf rdf:resource="#Global:Content"/>
        <rdfs:label xml:lang="en">Global Post</rdfs:label>
        <rdfs:comment xml:lang="en">Represents a generalized user-generated post.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:Post">
        <rdfs:subClassOf rdf:resource="#Global:Post"/>
        <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a generic post on the platform, including audience type and schedule time.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#Post</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#MP:Post">
        <rdfs:subClassOf rdf:resource="#Global:Post"/>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a specific post or piece of content created by a user.</MPcomment>
        <alignWithMP rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MicrobloggingPlatforms#Post</alignWithMP>
    </owl:Class>
    <owl:Class rdf:about="#OFB:Post">
        <rdfs:subClassOf rdf:resource="#Global:Post"/>
        <OFBcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents posts created by users, which may contain text, media, and other metadata.</OFBcomment>
        <alignWithOFB rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineForumBlogs#Post</alignWithOFB>
    </owl:Class>
    <owl:Class rdf:about="#OFB:ScheduledPost">
        <rdfs:subClassOf rdf:resource="#OFB:Post"/>
        <OFBcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents posts scheduled by users to be published at a future timestamp.</OFBcomment>
        <alignWithOFB rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineForumBlogs#ScheduledPost</alignWithOFB>
    </owl:Class>
    <owl:Class rdf:about="#OFB:CrossPostContent">
        <rdfs:subClassOf rdf:resource="#OFB:Post"/>
        <rdfs:label xml:lang="en">OFB Cross Post Content</rdfs:label>
        <rdfs:comment xml:lang="en">Content that has been cross-posted from another source within OFB.</rdfs:comment>
        <OFBcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents posts that are cross-posted from other communities or original posts.</OFBcomment> </owl:Class>
    <owl:Class rdf:about="#OFB:DraftContent">
        <rdfs:subClassOf rdf:resource="#OFB:Post"/>
        <rdfs:label xml:lang="en">OFB Draft Content</rdfs:label>
        <rdfs:comment xml:lang="en">Content saved as a draft within OFB.</rdfs:comment>
        <OFBcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents drafts saved by users, which can be edited and later posted.</OFBcomment> </owl:Class>
    <owl:Class rdf:about="#OSN:Post"> <rdfs:subClassOf rdf:resource="#Global:Post"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents posts created by users, including media posts, location check-ins, and live videos.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#Posts</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#OSN:BirthdayPost">
        <rdfs:subClassOf rdf:resource="#OSN:Post"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents posts created for birthdays.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#BirthdayPosts</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#OSN:AvatarPost">
        <rdfs:subClassOf rdf:resource="#OSN:Post"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents posts created using avatars.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#AvatarPosts</alignWithOSN>
    </owl:Class>

    <owl:Class rdf:about="#Global:VideoContent">
        <rdfs:subClassOf rdf:resource="#Global:Content"/>
        <rdfs:label xml:lang="en">Global Video Content</rdfs:label>
        <rdfs:comment xml:lang="en">Represents video content, including standard videos, short videos, and reels.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:Video">
        <rdfs:subClassOf rdf:resource="#Global:VideoContent"/>
        <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents video content in the system.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#Video</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:ShortVideo">
        <rdfs:subClassOf rdf:resource="#Global:VideoContent"/>
        <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents short video content in the system.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#ShortVideo</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#OSN:VideoContent">
        <rdfs:subClassOf rdf:resource="#Global:VideoContent"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The main class representing video-related content in the system, including video history and notifications.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#VideoContent</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#OSN:ReelVideo">
        <rdfs:subClassOf rdf:resource="#OSN:VideoContent"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a short video that is part of a reel.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#ReelVideo</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#OSN:ShortVideoContent">
        <rdfs:subClassOf rdf:resource="#OSN:VideoContent"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents short video content shared on social network platforms.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#ShortVideoContent</alignWithOSN>
    </owl:Class>

    <owl:Class rdf:about="#Global:LiveContent">
        <rdfs:subClassOf rdf:resource="#Global:Content"/>
        <rdfs:label xml:lang="en">Global Live Content</rdfs:label>
        <rdfs:comment xml:lang="en">Represents content generated during or associated with live streaming sessions.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:Live">
        <rdfs:subClassOf rdf:resource="#Global:LiveContent"/>
        <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents live streaming content within the system.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#Live</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:SuperChat">
        <rdfs:subClassOf rdf:resource="#MCSS:Live"/> <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a SuperChat donation event during live streaming.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#SuperChat</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#OSN:Live">
        <rdfs:subClassOf rdf:resource="#Global:LiveContent"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents live video broadcasts created by the user.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#LiveVideo</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#MP:Live">
        <rdfs:subClassOf rdf:resource="#Global:LiveContent"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents live video broadcasts created by the user.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#LiveVideo</alignWithOSN>
    </owl:Class>
        <owl:Class rdf:about="#MP:LiveInteractionActivity">
        <rdfs:subClassOf rdf:resource="#MP:Live"/>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents live interaction features on the platform, including live spaces, rooms, and upcoming sessions.</MPcomment>
        <alignWithMP rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MicrobloggingPlatforms#LiveInteractions</alignWithMP>
    </owl:Class>
    <owl:Class rdf:about="#MP:LiveSpacesRoomsActivity">
        <rdfs:subClassOf rdf:resource="#MP:Live"/>
        <rdfs:comment xml:lang="en">Activities related to MP Live Spaces/Rooms.</rdfs:comment>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents live spaces or rooms for users to listen, speak, and interact during live sessions.</MPcomment>
    </owl:Class>
        <owl:Class rdf:about="#MP:UpcomingSessionInteraction">
        <rdfs:subClassOf rdf:resource="#MP:Live"/>
        <rdfs:comment xml:lang="en">Interactions related to upcoming MP live sessions.</rdfs:comment>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents scheduled live sessions that users can set reminders for or add to their calendar.</MPcomment>
    </owl:Class>

    <!--Poll-->
    <owl:Class rdf:about="#Global:Poll">
        <rdfs:subClassOf rdf:resource="#Global:Content"/>
        <rdfs:label xml:lang="en">Global Poll</rdfs:label>
        <rdfs:comment xml:lang="en">Represents a poll created by users for interaction.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#OFB:Poll"> <rdfs:subClassOf rdf:resource="#Global:Poll"/>
        <OFBcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents polls created by users with questions and options for interaction.</OFBcomment>
        <alignWithOFB rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineForumBlogs#Polls</alignWithOFB>
    </owl:Class>
    <owl:Class rdf:about="#OSN:Poll"> <rdfs:subClassOf rdf:resource="#Global:Poll"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents polls created in a group.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#Polls</alignWithOSN>
    </owl:Class>

    <owl:Class rdf:about="#Global:Story">
        <rdfs:subClassOf rdf:resource="#Global:Content"/>
        <rdfs:label xml:lang="en">Global Story</rdfs:label>
        <rdfs:comment xml:lang="en">Represents temporary content, often visual, that expires after a period.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#OSN:Story"> <rdfs:subClassOf rdf:resource="#Global:Story"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents temporary stories shared by users, which expire after a certain period of time.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#Stories</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#OSN:StoryHighlight"> <rdfs:subClassOf rdf:resource="#OSN:Story"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents story highlights that can be saved beyond their expiration date.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#StoryHighlights</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:Story"> <rdfs:subClassOf rdf:resource="#Global:Story"/>
        <rdfs:comment xml:lang="en">Represents a story feature in MCSS platforms.</rdfs:comment>
    </owl:Class>

<!--Message-->
    <owl:Class rdf:about="#Global:Message">
        <rdfs:label xml:lang="en">Global Message</rdfs:label>
        <rdfs:comment xml:lang="en">Represents any form of message exchanged between users or in groups.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#Global:DirectMessage">
        <rdfs:subClassOf rdf:resource="#Global:Message"/>
        <rdfs:label xml:lang="en">Global Direct Message</rdfs:label>
        <rdfs:comment xml:lang="en">Represents a private message sent between users.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:DirectMessage"> <rdfs:subClassOf rdf:resource="#Global:DirectMessage"/>
        <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents the functionality and content of direct messages in the system.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#DirectMessages</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#OFB:DirectMessage"> <rdfs:subClassOf rdf:resource="#Global:DirectMessage"/>
        <OFBcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents one-on-one messaging interactions.</OFBcomment>
        <alignWithOFB rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineForumBlogs#DirectMessaging</alignWithOFB>
    </owl:Class>
    <owl:Class rdf:about="#OSN:DirectMessage"> <rdfs:subClassOf rdf:resource="#Global:DirectMessage"/>
        <OSNcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents direct messaging interactions among users on the platform.</OSNcomment>
        <alignWithOSN rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/OnlineSocialNetworkSites#DirectMessages</alignWithOSN>
    </owl:Class>
    <owl:Class rdf:about="#MP:DirectMessage"> <rdfs:subClassOf rdf:resource="#Global:DirectMessage"/>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents direct messages sent between users on the platform.</MPcomment>
        <alignWithMP rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MicrobloggingPlatforms#DirectMessages</alignWithMP>
    </owl:Class>

    <owl:Class rdf:about="#Global:GroupMessage">
        <rdfs:subClassOf rdf:resource="#Global:Message"/>
        <rdfs:label xml:lang="en">Global Group Message</rdfs:label>
        <rdfs:comment xml:lang="en">Represents a message sent within a group chat context.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:GroupMessage">
        <rdfs:subClassOf rdf:resource="#Global:GroupMessage"/>
        <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents group message threads and related interactions.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#GroupMessage</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#OSN:GroupMessage"> <rdfs:subClassOf rdf:resource="#Global:GroupMessage"/>
        <rdfs:comment xml:lang="en">Represents messages within an OSN group chat.</rdfs:comment>
        </owl:Class>

<!--Interaction-->
    <owl:Class rdf:about="#Global:ContentInteraction">
        <rdfs:label xml:lang="en">Global Content Interaction</rdfs:label>
        <rdfs:comment xml:lang="en">Represents any interaction a user has with content or other users related to content.</rdfs:comment>
    </owl:Class>
    <!--Share-->
    <owl:Class rdf:about="#Global:SharingAction">
        <rdfs:subClassOf rdf:resource="#Global:ContentInteraction"/>
        <rdfs:label xml:lang="en">Global Sharing Action</rdfs:label>
        <rdfs:comment xml:lang="en">Represents the action of sharing, reposting, or reblogging content.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="#MCSS:ShareAction"> <rdfs:subClassOf rdf:resource="#Global:SharingAction"/>
        <MCSScomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents a share interaction with content.</MCSScomment>
        <alignWithMCSS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MediaContentSharingSites#Share</alignWithMCSS>
    </owl:Class>
    <owl:Class rdf:about="#MP:ReblogRetweetAction"> <rdfs:subClassOf rdf:resource="#Global:SharingAction"/>
        <MPcomment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></MPcomment>
        <alignWithMP rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://saralutami.org/MicrobloggingPlatforms#ReblogRetweetPost</alignWithMP>
    </owl:Class>
    <owl:Class rdf:about="#OFB:CrossPostAction">
        <rdfs:subClassOf rdf:resource="#Global:SharingAction"/>
        <rdfs:label xml:lang="en">OFB Cross Post Action</rdfs:label>
        <rdfs:comment xml:lang="en">The action of creating a cross-post in OFB.</rdfs:comment>
        </owl:Class>
    <!--Comments-->
    <owl:Class rdf:about="#Global:CommentOrReplyAction">
        <rdfs:subClassOf rdf:resource="#Global:ContentInteraction"/>
        <rdfs:label xml:lang="en">Global Comment or Reply Action</rdfs:label>
