    <owl:Class rdf:about="#User">
        <owl:disjointWith rdf:resource="#ContentCreation"/>
        <owl:disjointWith rdf:resource="#DiscoveryAndSearch"/>
        <owl:disjointWith rdf:resource="#Notifications"/>
        <owl:disjointWith rdf:resource="#DirectMessages"/>
        <owl:disjointWith rdf:resource="#Monetization"/>
        <owl:disjointWith rdf:resource="#AccessibilityAndDisplay"/>
        <owl:disjointWith rdf:resource="#Community"/>
        <owl:disjointWith rdf:resource="#LiveInteractions"/>
        <owl:disjointWith rdf:resource="#Drafts"/>
        <owl:disjointWith rdf:resource="#Lists"/>
        <owl:disjointWith rdf:resource="#Bookmarks"/>
        <owl:disjointWith rdf:resource="#FollowerRequests"/>
    </owl:Class>

    <!-- Disjointness antar kelas ContentCreation -->
    <owl:Class rdf:about="#ContentCreation">
        <owl:disjointWith rdf:resource="#DiscoveryAndSearch"/>
        <owl:disjointWith rdf:resource="#Notifications"/>
        <owl:disjointWith rdf:resource="#DirectMessages"/>
        <owl:disjointWith rdf:resource="#Monetization"/>
        <owl:disjointWith rdf:resource="#AccessibilityAndDisplay"/>
        <owl:disjointWith rdf:resource="#Community"/>
        <owl:disjointWith rdf:resource="#LiveInteractions"/>
        <owl:disjointWith rdf:resource="#Drafts"/>
        <owl:disjointWith rdf:resource="#Lists"/>
        <owl:disjointWith rdf:resource="#Bookmarks"/>
        <owl:disjointWith rdf:resource="#FollowerRequests"/>
    </owl:Class>

    <!-- Disjointness antara subkelas dalam DiscoveryAndSearch -->
    <owl:Class rdf:about="#SearchFunctionality">
        <owl:disjointWith rdf:resource="#ExploreSettings"/>
    </owl:Class>

    <!-- Disjointness antara subkelas dalam Notifications -->
    <owl:Class rdf:about="#NotificationSettings">
        <owl:disjointWith rdf:resource="#Preferences"/>
    </owl:Class>

    <!-- Disjointness antara subkelas dalam Monetization -->
    <owl:Class rdf:about="#Subscription">
        <owl:disjointWith rdf:resource="#Advertisement"/>
    </owl:Class>

    <!-- Disjointness antara subkelas dalam LiveInteractions -->
    <owl:Class rdf:about="#LiveSpacesRooms">
        <owl:disjointWith rdf:resource="#UpcomingSessions"/>
    </owl:Class>

