<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    


<!-- Kelas User -->
    <owl:Class rdf:about="#User">
        <rdfs:comment>Represents a user in a microblogging platform (e.g., Twitter, Tumblr).</rdfs:comment>
    </owl:Class>

    <!-- Kelas ProfileSettings -->
    <owl:Class rdf:about="#ProfileSettings">
        <rdfs:subClassOf rdf:resource="#User"/>
        <rdfs:comment>Represents profile customization settings for the user.</rdfs:comment>
    </owl:Class>

    <!-- Data Property: hasEmail -->
    <owl:DatatypeProperty rdf:about="#hasEmail">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the email address associated with the user's account.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPassword -->
    <owl:DatatypeProperty rdf:about="#hasPassword">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the password for the user's account.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasBirthday -->
    <owl:DatatypeProperty rdf:about="#hasBirthday">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the user's birth date.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasSecuritySettings -->
    <owl:DatatypeProperty rdf:about="#hasSecuritySettings">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Contains the user's security settings (e.g., two-factor authentication status).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasSubscriptionStatus -->
    <owl:DatatypeProperty rdf:about="#hasSubscriptionStatus">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the user's subscription status (e.g., premium, free).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasNotificationsSetting -->
    <owl:DatatypeProperty rdf:about="#hasNotificationsSetting">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Specifies the user's notifications settings (e.g., email notifications enabled).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPrivacySetting -->
    <owl:DatatypeProperty rdf:about="#hasPrivacySetting">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Specifies the user's privacy settings (e.g., public profile, private account).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLogoutStatus -->
    <owl:DatatypeProperty rdf:about="#hasLogoutStatus">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates if the user is currently logged out of the platform.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Object Property: canEditProfile -->
    <owl:ObjectProperty rdf:about="#canEditProfile">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Indicates permission to edit profile information.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: canEditAvatar -->
    <owl:ObjectProperty rdf:about="#canEditAvatar">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates permission to edit profile avatar.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: canEditHeader -->
    <owl:ObjectProperty rdf:about="#canEditHeader">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates permission to edit profile header.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Property: hasBio -->
    <owl:DatatypeProperty rdf:about="#hasBio">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the bio or description on the user's profile.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLocation -->
    <owl:DatatypeProperty rdf:about="#hasLocation">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the user's location as shown on their profile.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasWebsite -->
    <owl:DatatypeProperty rdf:about="#hasWebsite">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Contains the website link provided by the user in their profile.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasBirthDate -->
    <owl:DatatypeProperty rdf:about="#hasBirthDate">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Records the user's birth date displayed on their profile.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasTips -->
    <owl:DatatypeProperty rdf:about="#hasTips">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores any tips or notable quotes the user chooses to display on their profile.</rdfs:comment>
    </owl:DatatypeProperty>


<!-- Kelas ContentCreation -->
    <owl:Class rdf:about="#ContentCreation">
        <rdfs:comment>Represents actions related to creating and managing content on a microblogging platform.</rdfs:comment>
    </owl:Class>

    <!-- Subclass: Post -->
    <owl:Class rdf:about="#Post">
        <rdfs:subClassOf rdf:resource="#ContentCreation"/>
        <rdfs:comment>Represents a specific post or piece of content created by a user.</rdfs:comment>
    </owl:Class>

    <!-- Data Property: hasPostURL -->
    <owl:DatatypeProperty rdf:about="#hasPostURL">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Contains the public URL or permalink to the post, if available.
            Useful for referencing the post directly from an external link.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostDeviceInfo -->
    <owl:DatatypeProperty rdf:about="#hasPostDeviceInfo">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Stores information about the device or app used to create the post
            (e.g., 'Twitter Web on MacBook Pro', 'Android App').
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostInteractionIP -->
    <owl:DatatypeProperty rdf:about="#hasPostInteractionIP">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Captures the IP address from which the user posted. 
            Useful for forensic tracing of post origin.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostMedia -->
    <owl:DatatypeProperty rdf:about="#hasPostMedia">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Holds the URL(s) or reference to media (images, videos, GIFs) 
            attached to the post.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostLocation -->
    <owl:DatatypeProperty rdf:about="#hasPostLocation">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Geographical location or place the user tagged 
            (e.g., 'New York, USA').
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostContent -->
    <owl:DatatypeProperty rdf:about="#hasPostContent">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Main textual content or body of the post, 
            including any caption or message written by the user.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostTags -->
    <owl:DatatypeProperty rdf:about="#hasPostTags">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Contains hashtags, labels, or additional tags associated with the post.
            Useful for categorizing or searching content by topic.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostTimestamp (replaces hasPostCreationTime) -->
    <owl:DatatypeProperty rdf:about="#hasPostTimestamp">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>
            Records the date and time when the post was created or published.
        </rdfs:comment>
    </owl:DatatypeProperty>


    <owl:ObjectProperty rdf:about="#postedBy">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>
        Indicates the user who created or published this post.
        </rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Property: hasPostStatus -->
    <owl:DatatypeProperty rdf:about="#hasPostStatus">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Indicates the publishing status of the post (e.g., "draft", "scheduled", or "published").
            Useful for distinguishing between different post states.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasScheduledTime -->
    <owl:DatatypeProperty rdf:about="#hasScheduledTime">
        <rdfs:domain rdf:resource="#Post"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>
            If the post status is "scheduled", records the date and time
            when the post will be published.
        </rdfs:comment>
    </owl:DatatypeProperty>



    <!-- Object Property: addToQueue -->
    <owl:ObjectProperty rdf:about="#addToQueue">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Allows a user to add a post to a queue for later publication.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: schedulePost -->
    <owl:ObjectProperty rdf:about="#schedulePost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Schedules a post to be published at a specified time.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: saveAsDraft -->
    <owl:ObjectProperty rdf:about="#saveAsDraft">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Saves a post as a draft for future editing or posting.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: postAsPrivate -->
    <owl:ObjectProperty rdf:about="#postAsPrivate">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Posts content with a private visibility setting.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: addContentLabel -->
    <owl:ObjectProperty rdf:about="#addContentLabel">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Adds a label or tag to the content for categorization or filtering.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: controlReblogRetweet -->
    <owl:ObjectProperty rdf:about="#controlReblogRetweet">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Enables or disables the option for other users to reblog or retweet the content.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: postMedia -->
    <owl:ObjectProperty rdf:about="#postMedia">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Posts media content such as images, videos, or GIFs.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: postLink -->
    <owl:ObjectProperty rdf:about="#postLink">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Posts a link within the content.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

<!-- Content Interaction Class -->
    <owl:Class rdf:about="#ContentInteraction"/>
    
    <!-- Likes Sub-class -->
    <owl:Class rdf:about="#Likes">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for Likes -->
    <owl:ObjectProperty rdf:about="#likesPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesReblogRetweetPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ReblogRetweetPost"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesReplies">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Reply"/>
    </owl:ObjectProperty>

    <!-- ReblogRetweetPost Sub-class -->
    <owl:Class rdf:about="#ReblogRetweetPost">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for ReblogRetweetPost -->
    <owl:ObjectProperty rdf:about="#reblogsRetweetsA">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reblogsRetweetsComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reblogsRetweetsAgainA">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ReblogRetweetPost"/>
    </owl:ObjectProperty>

    <!-- Comment Sub-class -->
    <owl:Class rdf:about="#Comment">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for Comment -->
    <owl:ObjectProperty rdf:about="#commentsOn">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#commentsOnReblogRetweetPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ReblogRetweetPost"/>
    </owl:ObjectProperty>

    <!-- Data Property: hasCommentContent -->
    <owl:DatatypeProperty rdf:about="#hasCommentContent">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Holds the main text/body of a comment posted by a user.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasCommentTimestamp -->
    <owl:DatatypeProperty rdf:about="#hasCommentTimestamp">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>
            The date and time when this comment was created.
            Useful for forensic timeline reconstruction.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasCommentIP -->
    <owl:DatatypeProperty rdf:about="#hasCommentIP">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            The IP address of the device used to post the comment.
            Critical for digital forensics or geolocation.
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasCommentDevice -->
    <owl:DatatypeProperty rdf:about="#hasCommentDevice">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>
            Additional info about the device/app used (e.g., 'Twitter Web', 'Android App').
        </rdfs:comment>
    </owl:DatatypeProperty>

    <!-- ObjectProperty: commentedBy (siapa yang membuat komentar) -->
    <owl:ObjectProperty rdf:about="#commentedBy">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>
            Points to the user who wrote this comment.
        </rdfs:comment>
    </owl:ObjectProperty>

    <!-- ObjectProperty: commentedOn (posting atau target komentar) -->
    <owl:ObjectProperty rdf:about="#commentedOn">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>
            Points to the post on which this comment was made.
            For reblog/retweet, you could similarly add commentedOnReblogRetweetPost, etc.
        </rdfs:comment>
    </owl:ObjectProperty>



    <!-- Reply Sub-class -->
    <owl:Class rdf:about="#Reply">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for Reply -->
    <owl:ObjectProperty rdf:about="#repliesOnComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#deletesReblogRetweetPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Reply"/>
    </owl:ObjectProperty>

    <!-- Block Sub-class -->
    <owl:Class rdf:about="#Block">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- ObjectProperty: blockInitiator -->
    <owl:ObjectProperty rdf:about="#blockInitiator">
        <rdfs:domain rdf:resource="#Block"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>
            The user who initiates the block action.
        </rdfs:comment>
    </owl:ObjectProperty>

    <!-- ObjectProperty: blockedUser -->
    <owl:ObjectProperty rdf:about="#blockedUser">
        <rdfs:domain rdf:resource="#Block"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>
            The user who is being blocked.
        </rdfs:comment>
    </owl:ObjectProperty>


    <!-- Object Properties for Block -->
    <owl:ObjectProperty rdf:about="#blocks">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#unblocks">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>

    <!-- Report Sub-class -->
    <owl:Class rdf:about="#Report">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for Report -->
    <owl:ObjectProperty rdf:about="#reports">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reportPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reportsReply">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Reply"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reportComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>

    <!-- FollowUnfollow Sub-class -->
    <owl:Class rdf:about="#FollowUnfollow">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for FollowUnfollow -->
    <owl:ObjectProperty rdf:about="#follow">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#unfollow">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>

    <!-- Ask Sub-class -->
    <owl:Class rdf:about="#Ask">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Property for Ask -->
    <owl:ObjectProperty rdf:about="#asks">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>

    <!-- Likes Sub-class -->
    <owl:Class rdf:about="#Post">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Characteristics for Object Properties -->
    <rdf:Description rdf:about="#likesPost">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#reblogsRetweetsA">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#commentsOn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#blocks">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#reports">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
