    <!-- Object Properties for Comment -->
    <owl:ObjectProperty rdf:about="#commentsOn">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#commentsOnReblogRetweetPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ReblogRetweetPost"/>
    </owl:ObjectProperty>

    <!-- Reply Sub-class -->
    <owl:Class rdf:about="#Reply">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for Reply -->
    <owl:ObjectProperty rdf:about="#repliesOnComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#deletesReblogRetweetPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Reply"/>
    </owl:ObjectProperty>

    <!-- Block Sub-class -->
    <owl:Class rdf:about="#Block">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for Block -->
    <owl:ObjectProperty rdf:about="#blocks">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#unblocks">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>

    <!-- Report Sub-class -->
    <owl:Class rdf:about="#Report">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for Report -->
    <owl:ObjectProperty rdf:about="#reports">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reportPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reportsReply">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Reply"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reportComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>

    <!-- FollowUnfollow Sub-class -->
    <owl:Class rdf:about="#FollowUnfollow">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for FollowUnfollow -->
    <owl:ObjectProperty rdf:about="#follow">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#unfollow">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>

    <!-- Ask Sub-class -->
    <owl:Class rdf:about="#Ask">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Property for Ask -->
    <owl:ObjectProperty rdf:about="#asks">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>

    <!-- Likes Sub-class -->
    <owl:Class rdf:about="#Post">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Characteristics for Object Properties -->
    <rdf:Description rdf:about="#likesPost">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#reblogsRetweetsA">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#commentsOn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#blocks">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#reports">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </rdf:Description>
    <rdf:Description rdf:about="#follow">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
    </rdf:Description>

    <!-- Words Sub-class -->
    <owl:Class rdf:about="#Words">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for BlockingAndMuting -->
    <owl:ObjectProperty rdf:about="#mutesNotifications">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Notifications"/>
        <rdfs:comment>Indicates that a user mutes notifications.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#mutesWords">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Words"/>
        <rdfs:comment>Indicates that a user mutes specific words.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#mutesAccounts">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Indicates that a user mutes another user account.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

<!-- Kelas DiscoveryAndSearch -->
    <owl:Class rdf:about="#DiscoveryAndSearch">
        <rdfs:comment>Represents features for discovering and searching content, people, and topics on a microblogging platform.</rdfs:comment>
    </owl:Class>

    <!-- Sub-kelas SearchFunctionality -->
    <owl:Class rdf:about="#SearchFunctionality">
        <rdfs:subClassOf rdf:resource="#DiscoveryAndSearch"/>
        <rdfs:comment>Represents search functionalities within the platform.</rdfs:comment>
    </owl:Class>

    <!-- Sub-kelas ExploreSettings -->
    <owl:Class rdf:about="#ExploreSettings">
        <rdfs:subClassOf rdf:resource="#DiscoveryAndSearch"/>
        <rdfs:comment>Represents settings for exploring content based on location and personalization.</rdfs:comment>
    </owl:Class>

    <!-- Object Property: searchFor -->
    <owl:ObjectProperty rdf:about="#searchFor">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Allows a user to search for other users within the platform.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: searchTopicsOn -->
    <owl:ObjectProperty rdf:about="#searchTopicsOn">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#SearchFunctionality"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Allows a user to search for specific topics within the platform.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: saveSearches -->
    <owl:ObjectProperty rdf:about="#saveSearchesFor">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#SearchFunctionality"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Allows a user to save their searches for future reference.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: useAdvancedSearch -->
    <owl:ObjectProperty rdf:about="#useAdvancedSearchOn">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#SearchFunctionality"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Allows a user to use advanced search options for more specific query parameters.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- [RAGU]Object Property: showContentBasedOnLocation -->
    <owl:ObjectProperty rdf:about="#searchContentBasedOnLocationOn">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#SearchFunctionality"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Displays content based on the userâ€™s location.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- [RAGU]Object Property: personalizeTrends -->
    <owl:ObjectProperty rdf:about="#personalizeTrendsOn">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ExploreSettings"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Personalizes trending content for the user based on their interests.</rdfs:comment>
    </owl:ObjectProperty>

<!-- Kelas Notifications -->
    <owl:Class rdf:about="#Notifications">
        <rdfs:comment>Represents user notifications and their related settings on the platform.</rdfs:comment>
    </owl:Class>

    <!-- Sub-kelas NotificationSettings -->
    <owl:Class rdf:about="#NotificationSettings">
        <rdfs:subClassOf rdf:resource="#Notifications"/>
        <rdfs:comment>Represents settings related to notifications that the user can configure.</rdfs:comment>
    </owl:Class>

    <!-- Sub-kelas Preferences -->
    <owl:Class rdf:about="#Preferences">
        <rdfs:subClassOf rdf:resource="#Notifications"/>
        <rdfs:comment>Represents user-specific notification preferences, such as unread counts and badges.</rdfs:comment>
    </owl:Class>

    <!-- Data Property: hasQualityFilter -->
    <owl:DatatypeProperty rdf:about="#hasQualityFilter">
        <rdfs:domain rdf:resource="#NotificationSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates if the quality filter is enabled for notifications.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: receivesPushNotifications -->
    <owl:DatatypeProperty rdf:about="#receivesPushNotifications">
        <rdfs:domain rdf:resource="#NotificationSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates if the user receives push notifications.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: receivesSMSNotifications -->
    <owl:DatatypeProperty rdf:about="#receivesSMSNotifications">
        <rdfs:domain rdf:resource="#NotificationSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates if the user receives SMS notifications.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: receivesEmailNotifications -->
    <owl:DatatypeProperty rdf:about="#receivesEmailNotifications">
        <rdfs:domain rdf:resource="#NotificationSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates if the user receives email notifications.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasUnreadNotificationCountBadge -->
    <owl:DatatypeProperty rdf:about="#hasUnreadNotificationCountBadge">
        <rdfs:domain rdf:resource="#Preferences"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Displays the count of unread notifications as a badge for the user.</rdfs:comment>
    </owl:DatatypeProperty>

<!-- Kelas DirectMessages -->
