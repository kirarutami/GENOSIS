<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

<!-- Kelas User -->
    <owl:Class rdf:about="#User">
        <rdfs:comment>Represents a user in a microblogging platform (e.g., Twitter, Tumblr).</rdfs:comment>
    </owl:Class>

    <!-- Kelas ProfileSettings -->
    <owl:Class rdf:about="#ProfileSettings">
        <rdfs:subClassOf rdf:resource="#User"/>
        <rdfs:comment>Represents profile customization settings for the user.</rdfs:comment>
    </owl:Class>

    <!-- Data Property: hasEmail -->
    <owl:DatatypeProperty rdf:about="#hasEmail">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the email address associated with the user's account.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPassword -->
    <owl:DatatypeProperty rdf:about="#hasPassword">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the password for the user's account.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasBirthday -->
    <owl:DatatypeProperty rdf:about="#hasBirthday">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the user's birth date.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasSecuritySettings -->
    <owl:DatatypeProperty rdf:about="#hasSecuritySettings">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Contains the user's security settings (e.g., two-factor authentication status).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasSubscriptionStatus -->
    <owl:DatatypeProperty rdf:about="#hasSubscriptionStatus">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the user's subscription status (e.g., premium, free).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasNotificationsSetting -->
    <owl:DatatypeProperty rdf:about="#hasNotificationsSetting">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Specifies the user's notifications settings (e.g., email notifications enabled).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPrivacySetting -->
    <owl:DatatypeProperty rdf:about="#hasPrivacySetting">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Specifies the user's privacy settings (e.g., public profile, private account).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLogoutStatus -->
    <owl:DatatypeProperty rdf:about="#hasLogoutStatus">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates if the user is currently logged out of the platform.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Object Property: canEditProfile -->
    <owl:ObjectProperty rdf:about="#canEditProfile">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Indicates permission to edit profile information.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: canEditAvatar -->
    <owl:ObjectProperty rdf:about="#canEditAvatar">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates permission to edit profile avatar.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: canEditHeader -->
    <owl:ObjectProperty rdf:about="#canEditHeader">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates permission to edit profile header.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Property: hasBio -->
    <owl:DatatypeProperty rdf:about="#hasBio">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the bio or description on the user's profile.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLocation -->
    <owl:DatatypeProperty rdf:about="#hasLocation">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the user's location as shown on their profile.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasWebsite -->
    <owl:DatatypeProperty rdf:about="#hasWebsite">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Contains the website link provided by the user in their profile.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasBirthDate -->
    <owl:DatatypeProperty rdf:about="#hasBirthDate">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Records the user's birth date displayed on their profile.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasTips -->
    <owl:DatatypeProperty rdf:about="#hasTips">
        <rdfs:domain rdf:resource="#ProfileSettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores any tips or notable quotes the user chooses to display on their profile.</rdfs:comment>
    </owl:DatatypeProperty>


<!-- Kelas ContentCreation -->
    <owl:Class rdf:about="#ContentCreation">
        <rdfs:comment>Represents actions related to creating and managing content on a microblogging platform.</rdfs:comment>
    </owl:Class>

    <!-- Object Property: makePost -->
    <owl:ObjectProperty rdf:about="#makePost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Indicates the action of creating a new post by the user.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: addToQueue -->
    <owl:ObjectProperty rdf:about="#addToQueue">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Allows a user to add a post to a queue for later publication.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: schedulePost -->
    <owl:ObjectProperty rdf:about="#schedulePost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Schedules a post to be published at a specified time.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: saveAsDraft -->
    <owl:ObjectProperty rdf:about="#saveAsDraft">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Saves a post as a draft for future editing or posting.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: postAsPrivate -->
    <owl:ObjectProperty rdf:about="#postAsPrivate">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Posts content with a private visibility setting.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: addContentLabel -->
    <owl:ObjectProperty rdf:about="#addContentLabel">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Adds a label or tag to the content for categorization or filtering.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: controlReblogRetweet -->
    <owl:ObjectProperty rdf:about="#controlReblogRetweet">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Enables or disables the option for other users to reblog or retweet the content.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: postMedia -->
    <owl:ObjectProperty rdf:about="#postMedia">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Posts media content such as images, videos, or GIFs.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Object Property: postLink -->
    <owl:ObjectProperty rdf:about="#postLink">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ContentCreation"/>
        <rdfs:comment>Posts a link within the content.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

<!-- Content Interaction Class -->
    <owl:Class rdf:about="#ContentInteraction"/>
    
    <!-- Likes Sub-class -->
    <owl:Class rdf:about="#Likes">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Object Properties for Likes -->
    <owl:ObjectProperty rdf:about="#likesPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesReblogRetweetPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ReblogRetweetPost"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesReplies">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Reply"/>
    </owl:ObjectProperty>

    <!-- ReblogRetweetPost Sub-class -->
    <owl:Class rdf:about="#ReblogRetweetPost">
