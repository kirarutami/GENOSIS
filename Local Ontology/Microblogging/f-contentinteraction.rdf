    </owl:Class>

    <!-- Words Sub-class -->
    <owl:Class rdf:about="#Words">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <owl:disjointWith rdf:resource="#ReblogRetweetPost"/>
        <owl:disjointWith rdf:resource="#Comment"/>
        <owl:disjointWith rdf:resource="#Reply"/>
        <owl:disjointWith rdf:resource="#Block"/>
        <owl:disjointWith rdf:resource="#Report"/>
        <owl:disjointWith rdf:resource="#FollowUnfollow"/>
        <owl:disjointWith rdf:resource="#Ask"/>
        <owl:disjointWith rdf:resource="#Post"/>
    </owl:Class>

    <!-- Object Properties for BlockingAndMuting -->
    <owl:ObjectProperty rdf:about="#mutesNotifications">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Notifications"/>
        <rdfs:comment>Indicates that a user mutes notifications.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#mutesWords">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Words"/>
        <rdfs:comment>Indicates that a user mutes specific words.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#mutesAccounts">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Indicates that a user mutes another user account.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Characteristics for Object Properties -->
    <rdf:Description rdf:about="#likesPost">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
