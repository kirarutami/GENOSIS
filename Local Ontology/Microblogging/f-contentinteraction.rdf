        <owl:disjointWith rdf:resource="#Block"/>
        <owl:disjointWith rdf:resource="#Report"/>
        <owl:disjointWith rdf:resource="#FollowUnfollow"/>
        <owl:disjointWith rdf:resource="#Ask"/>
    </owl:Class>

    <!-- Object Properties for ReblogRetweetPost -->
    <owl:ObjectProperty rdf:about="#reblogsRetweetsA">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reblogsRetweetsComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#reblogsRetweetsAgainA">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ReblogRetweetPost"/>
