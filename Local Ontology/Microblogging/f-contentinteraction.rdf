<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- Content Interaction Class -->
    <owl:Class rdf:about="#ContentInteraction"/>
    
    <!-- Likes Sub-class -->
    <owl:Class rdf:about="#Likes">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <owl:disjointWith rdf:resource="#ReblogRetweetPost"/>
        <owl:disjointWith rdf:resource="#Comment"/>
        <owl:disjointWith rdf:resource="#Reply"/>
        <owl:disjointWith rdf:resource="#Block"/>
        <owl:disjointWith rdf:resource="#Report"/>
        <owl:disjointWith rdf:resource="#FollowUnfollow"/>
        <owl:disjointWith rdf:resource="#Ask"/>
    </owl:Class>

    <!-- Object Properties for Likes -->
    <owl:ObjectProperty rdf:about="#likesPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesReblogRetweetPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ReblogRetweetPost"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#likesReplies">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Reply"/>
    </owl:ObjectProperty>

