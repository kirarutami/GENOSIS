<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    
    <!-- Class: VideoContent -->
    <owl:Class rdf:about="#VideoContent">
        <rdfs:comment>The main class representing video-related content in the system, including video history and notifications.</rdfs:comment>
    </owl:Class>

    <!-- Sub-class: Videos -->
    <owl:Class rdf:about="#Videos">
        <rdfs:comment>Represents video content watched by users, including viewing history and notifications.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#VideoContent"/>
    </owl:Class>

    <!-- Data Property: hasVideoHistory -->
    <owl:DatatypeProperty rdf:about="#hasVideoHistory">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the video watch history for a user.</rdfs:comment>
        <rdfs:domain rdf:resource="#Videos"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasVideoWatchTimestamp -->
    <owl:DatatypeProperty rdf:about="#hasVideoWatchTimestamp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the timestamp when the video was watched.</rdfs:comment>
        <rdfs:domain rdf:resource="#hasVideoHistory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasVideoWatchDuration -->
    <owl:DatatypeProperty rdf:about="#hasVideoWatchDuration">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the duration for which the video was watched.</rdfs:comment>
        <rdfs:domain rdf:resource="#hasVideoHistory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasVideoWatchIP -->
    <owl:DatatypeProperty rdf:about="#hasVideoWatchIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP address of the user who watched the video.</rdfs:comment>
        <rdfs:domain rdf:resource="#hasVideoHistory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasVideoQuality -->
    <owl:DatatypeProperty rdf:about="#hasVideoQuality">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the quality of the video that was played (e.g., 1080p, 720p).</rdfs:comment>
        <rdfs:domain rdf:resource="#hasVideoHistory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasVideoDeviceInfo -->
    <owl:DatatypeProperty rdf:about="#hasVideoDeviceInfo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the device information used to watch the video.</rdfs:comment>
        <rdfs:domain rdf:resource="#hasVideoHistory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Sub-class: VideoNotifications -->
    <owl:Class rdf:about="#VideoNotifications">
        <rdfs:comment>Represents notifications related to video content, such as new video uploads, reminders, and other updates.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Videos"/>
    </owl:Class>

    <!-- Data Property: hasNotificationTimestamp -->
    <owl:DatatypeProperty rdf:about="#hasNotificationTimestamp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the timestamp when the video notification was received.</rdfs:comment>
        <rdfs:domain rdf:resource="#VideoNotifications"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasNotificationStatus -->
    <owl:DatatypeProperty rdf:about="#hasNotificationStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the video notification is active or inactive.</rdfs:comment>
        <rdfs:domain rdf:resource="#VideoNotifications"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasNotificationType -->
    <owl:DatatypeProperty rdf:about="#hasNotificationType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the type of video notification, such as new upload, reminder, or other alerts.</rdfs:comment>
        <rdfs:domain rdf:resource="#VideoNotifications"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasNotificationIP -->
    <owl:DatatypeProperty rdf:about="#hasNotificationIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP address of the user who received the video notification.</rdfs:comment>
        <rdfs:domain rdf:resource="#VideoNotifications"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasNotificationDeviceInfo -->
    <owl:DatatypeProperty rdf:about="#hasNotificationDeviceInfo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the device information used when the video notification was received.</rdfs:comment>
        <rdfs:domain rdf:resource="#VideoNotifications"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

</rdf:RDF>