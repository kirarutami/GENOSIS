    <owl:ObjectProperty rdf:about="#setsMemberToAdmin">
        <rdfs:domain rdf:resource="#Moderator"/>
        <rdfs:range rdf:resource="#Admin"/>
        <rdfs:comment>Allows a moderator to assign a user as an admin.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#removesAdmin">
        <rdfs:domain rdf:resource="#Moderator"/>
        <rdfs:range rdf:resource="#Admin"/>
        <rdfs:comment>Allows a moderator to remove an admin role from a user.</rdfs:comment>
