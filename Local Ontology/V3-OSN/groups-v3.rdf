<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- Groups Class -->
    <owl:Class rdf:about="#Groups">
        <rdfs:comment>Represents groups in the online social network site.</rdfs:comment>
    </owl:Class>

    <!-- Data Properties for Groups -->
    <owl:DatatypeProperty rdf:about="#hasGroupName">
        <rdfs:domain rdf:resource="#Groups"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of the group.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGroupDescription">
        <rdfs:domain rdf:resource="#Groups"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A description of the group.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasMembershipRequestsCount">
        <rdfs:domain rdf:resource="#Groups"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>The number of membership requests for the group.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGroupCategory">
        <rdfs:domain rdf:resource="#Groups"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The category of the group (e.g., Travel, Education).</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGroupCreationTimestamp">
        <rdfs:domain rdf:resource="#Groups"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The creation timestamp of the group.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasLastActivityTimestamp">
        <rdfs:domain rdf:resource="#Groups"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The timestamp of the last activity in the group.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGroupCoverPhoto">
        <rdfs:domain rdf:resource="#Groups"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment>The cover photo of the group.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- GroupSettings Sub-class -->
    <owl:Class rdf:about="#GroupSettings">
        <rdfs:subClassOf rdf:resource="#Groups"/>
        <rdfs:comment>Represents the settings of a group.</rdfs:comment>
