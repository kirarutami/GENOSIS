    <owl:DatatypeProperty rdf:about="#hasViolationReportedTimestamp">
        <rdfs:domain rdf:resource="#Violations"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
