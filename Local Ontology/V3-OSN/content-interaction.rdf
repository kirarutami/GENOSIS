<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
         
    <!-- Class: ContentInteraction -->
    <owl:Class rdf:about="#ContentInteraction">
        <rdfs:comment>The class representing user interactions with content, such as likes, reactions, and comments.</rdfs:comment>
        <owl:disjointWith rdf:resource="#ContentCreation"/>
        <owl:disjointWith rdf:resource="#User"/>
    </owl:Class>

    <!-- Sub-class: LikesReactions -->
    <owl:Class rdf:about="#LikesReactions">
        <rdfs:comment>Represents user interactions involving likes and reactions to content.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <owl:disjointWith rdf:resource="#Comments"/>
    </owl:Class>

    <!-- Sub-class: Like -->
    <owl:Class rdf:about="#Like">
        <rdfs:comment>Represents a user's 'like' interaction on a post or content.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#LikesReactions"/>
    </owl:Class>

    <!-- Data Property: hasTimestamp (for Like) -->
    <owl:DatatypeProperty rdf:about="#hasLikeTimestamp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the timestamp when the like was given.</rdfs:comment>
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasUserInteractionIP (for Like) -->
    <owl:DatatypeProperty rdf:about="#hasUserInteractionIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP address of the user who gave the like.</rdfs:comment>
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasDeviceInfo (for Like) -->
    <owl:DatatypeProperty rdf:about="#hasDeviceInfo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the device information of the user who gave the like.</rdfs:comment>
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Sub-class: React -->
    <owl:Class rdf:about="#React">
        <rdfs:comment>Represents a user's reaction to a post, such as love, angry, etc.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#LikesReactions"/>
    </owl:Class>

    <!-- Data Property: hasReactionType (for React) -->
    <owl:DatatypeProperty rdf:about="#hasReactionType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the type of reaction given, such as Love, Angry, etc.</rdfs:comment>
        <rdfs:domain rdf:resource="#React"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasTimestamp (for React) -->
    <owl:DatatypeProperty rdf:about="#hasReactTimestamp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the timestamp when the reaction was given.</rdfs:comment>
        <rdfs:domain rdf:resource="#React"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasUserInteractionIP (for React) -->
    <owl:DatatypeProperty rdf:about="#hasUserInteractionIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP address of the user who gave the reaction.</rdfs:comment>
        <rdfs:domain rdf:resource="#React"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasDeviceInfo (for React) -->
    <owl:DatatypeProperty rdf:about="#hasDeviceInfo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the device information of the user who gave the reaction.</rdfs:comment>
        <rdfs:domain rdf:resource="#React"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Sub-class: Comments -->
    <owl:Class rdf:about="#Comments">
        <rdfs:comment>Represents user interactions involving comments on content.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
    </owl:Class>

    <!-- Sub-class: TextComment -->
    <owl:Class rdf:about="#TextComment">
        <rdfs:comment>Represents a text-based comment made by a user.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Comments"/>
    </owl:Class>

    <!-- Data Property: hasCommentText (for TextComment) -->
    <owl:DatatypeProperty rdf:about="#hasCommentText">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the text of the comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#TextComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasTimestamp (for TextComment) -->
    <owl:DatatypeProperty rdf:about="#hasCommentTimestamp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the timestamp when the comment was made.</rdfs:comment>
        <rdfs:domain rdf:resource="#TextComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasUserInteractionIP (for TextComment) -->
    <owl:DatatypeProperty rdf:about="#hasUserInteractionIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP address of the user who made the comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#TextComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasDeviceInfo (for TextComment) -->
    <owl:DatatypeProperty rdf:about="#hasDeviceInfo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the device information of the user who made the comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#TextComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Sub-class: GIFComment -->
    <owl:Class rdf:about="#GIFComment">
        <rdfs:comment>Represents a comment made by a user that contains a GIF.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Comments"/>
    </owl:Class>

    <!-- Data Property: hasGIFLink (for GIFComment) -->
    <owl:DatatypeProperty rdf:about="#hasGIFLink">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the link to the GIF used in the comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#GIFComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasTimestamp (for GIFComment) -->
    <owl:DatatypeProperty rdf:about="#hasGIFCommentTimestamp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the timestamp when the GIF comment was made.</rdfs:comment>
        <rdfs:domain rdf:resource="#GIFComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasUserInteractionIP (for GIFComment) -->
    <owl:DatatypeProperty rdf:about="#hasUserInteractionIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP address of the user who made the GIF comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#GIFComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasDeviceInfo (for GIFComment) -->
    <owl:DatatypeProperty rdf:about="#hasDeviceInfo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the device information of the user who made the GIF comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#GIFComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Sub-class: EmojiComment -->
    <owl:Class rdf:about="#EmojiComment">
        <rdfs:comment>Represents a comment made by a user that contains an emoji.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Comments"/>
    </owl:Class>

    <!-- Data Property: hasEmojiCode (for EmojiComment) -->
    <owl:DatatypeProperty rdf:about="#hasEmojiCode">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the code of the emoji used in the comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#EmojiComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasTimestamp (for EmojiComment) -->
    <owl:DatatypeProperty rdf:about="#hasEmojiTimestamp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the timestamp when the emoji comment was made.</rdfs:comment>
        <rdfs:domain rdf:resource="#EmojiComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasUserInteractionIP (for EmojiComment) -->
    <owl:DatatypeProperty rdf:about="#hasUserInteractionIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP address of the user who made the emoji comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#EmojiComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasDeviceInfo (for EmojiComment) -->
    <owl:DatatypeProperty rdf:about="#hasDeviceInfo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the device information of the user who made the emoji comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#EmojiComment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Object Property: associatedWithContent -->
    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:comment>Links an interaction (like, react, comment, etc.) to the associated content (post, story, reel).</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: associatedWithGroup -->
    <owl:ObjectProperty rdf:about="#associatedWithGroup">
        <rdfs:comment>Links an interaction to the group in which the content was posted.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Linking Likes to Content -->
    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a like action to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Linking Reactions to Content -->
    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#React"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a reaction (love, angry, etc.) to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Linking Comments to Content -->
    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Comments"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a comment action to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Linking TextComments to Content -->
    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#TextComment"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a text-based comment to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Linking GIFComments to Content -->
    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#GIFComment"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a GIF-based comment to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Linking EmojiComments to Content -->
    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#EmojiComment"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links an emoji-based comment to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Linking Interactions to Groups -->
    <owl:ObjectProperty rdf:about="#associatedWithGroup">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="#Group"/>
        <rdfs:comment>Links a like to the group where the post was shared.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithGroup">
        <rdfs:domain rdf:resource="#React"/>
        <rdfs:range rdf:resource="#Group"/>
        <rdfs:comment>Links a reaction to the group where the content was posted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithGroup">
        <rdfs:domain rdf:resource="#Comments"/>
        <rdfs:range rdf:resource="#Group"/>
        <rdfs:comment>Links a comment to the group where the content was posted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithGroup">
        <rdfs:domain rdf:resource="#TextComment"/>
        <rdfs:range rdf:resource="#Group"/>
        <rdfs:comment>Links a text comment to the group where the content was posted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithGroup">
        <rdfs:domain rdf:resource="#GIFComment"/>
        <rdfs:range rdf:resource="#Group"/>
        <rdfs:comment>Links a GIF comment to the group where the content was posted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithGroup">
        <rdfs:domain rdf:resource="#EmojiComment"/>
        <rdfs:range rdf:resource="#Group"/>
        <rdfs:comment>Links an emoji comment to the group where the content was posted.</rdfs:comment>
    </owl:ObjectProperty>


</rdf:RDF>