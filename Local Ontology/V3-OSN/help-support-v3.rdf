<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- HelpSupport Class -->
    <owl:Class rdf:about="#HelpSupport">
        <rdfs:comment>Represents support and help-related features for users.</rdfs:comment>
    </owl:Class>

    <owl:ObjectProperty rdf:about="#accessHelpCenter">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#HelpSupport"/>
        <rdfs:comment>Links a user to accessing the help center.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#submitsReportProblem">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#HelpSupport"/>
        <rdfs:comment>Links a user to submitting a problem report.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#accessSupportInbox">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#HelpSupport"/>
        <rdfs:comment>Links a user to accessing the support inbox.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasSupportAccessTimestamp">
        <rdfs:domain rdf:resource="#HelpSupport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp for accessing the support center.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasReportTimestamp">
        <rdfs:domain rdf:resource="#HelpSupport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp for submitting a problem report.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasInboxAccessTimestamp">
        <rdfs:domain rdf:resource="#HelpSupport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp for accessing the support inbox.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- ProfessionalAccess Class -->
    <owl:Class rdf:about="#ProfessionalAccess">
        <rdfs:comment>Represents professional access features and services.</rdfs:comment>
    </owl:Class>

    <!-- MetaVerified Sub-Class -->
    <owl:Class rdf:about="#MetaVerified">
        <rdfs:subClassOf rdf:resource="#ProfessionalAccess"/>
        <rdfs:comment>Handles verification and public presence-related features for users.</rdfs:comment>
    </owl:Class>

    <owl:DatatypeProperty rdf:about="#hasPublicPresenceStatus">
        <rdfs:domain rdf:resource="#MetaVerified"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the user has a verified public presence.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- AlsoFromMeta Class -->
    <owl:Class rdf:about="#AlsoFromMeta">
        <rdfs:comment>Represents other applications and services provided by Meta.</rdfs:comment>
    </owl:Class>

    <!-- OtherApps Sub-Class -->
    <owl:Class rdf:about="#OtherApps">
        <rdfs:subClassOf rdf:resource="#AlsoFromMeta"/>
        <rdfs:comment>Represents other Meta apps that can connect to the user's account.</rdfs:comment>
    </owl:Class>

    <owl:ObjectProperty rdf:about="#Messenger">
        <rdfs:domain rdf:resource="#OtherApps"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Links the user to the Messenger app.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#WhatsApp">
        <rdfs:domain rdf:resource="#OtherApps"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Links the user to the WhatsApp app.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasConnectedAppStatus">
        <rdfs:domain rdf:resource="#OtherApps"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Status indicating whether the app is connected to the user's account.</rdfs:comment>
    </owl:DatatypeProperty>

</rdf:RDF>
