<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    

<!-- Class: User -->
    <owl:Class rdf:about="#User">
        <rdfs:comment>The primary class representing individual users in the system.</rdfs:comment>
    </owl:Class>

    <!-- Sub-Class: ProfileSettings -->
    <owl:Class rdf:about="#ProfileSettings">
        <rdfs:subClassOf rdf:resource="#User"/>
        <rdfs:comment>Sub-class representing settings a user can edit in their profile.</rdfs:comment>
    </owl:Class>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="#hasUserID">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Unique identifier for the user.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasUsername">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The username chosen by the user.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasEmail">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The email address associated with the user.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasPhoneNumber">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The phone number associated with the user.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasProfilePicture">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment>URL of the user's profile picture.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasAccountCreationDate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The date and time when the user's account was created.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasLastLoginDate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The date and time of the user's last login.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasPasswordHash">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Hashed password of the user.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasTwoFactorAuthenticationStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Status of two-factor authentication for the user (enabled/disabled).</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasSubscriptionStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the user has an active subscription.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasPremiumMembershipStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the user is a premium member.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasPrivacySettings">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Privacy settings defined by the user.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasNotificationPreferences">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Notification preferences set by the user.</rdfs:comment>
    </owl:DatatypeProperty>
    <!-- Object Property: ownsChannel -->
    <owl:ObjectProperty rdf:about="#ownsChannel">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Channel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user owns a specific channel on the platform.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: managesChannel -->
    <owl:ObjectProperty rdf:about="#managesChannel">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Channel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Indicates that a user manages (collaborates on) a specific channel on the platform.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Property: hasAccessibilityPreferences -->
    <owl:DatatypeProperty rdf:about="#hasAccessibilityPreferences">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Specifies the accessibility preferences of the user, such as screen reader settings or text size.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Sub-class: ProfileSettings -->
    <owl:Class rdf:about="#ProfileSettings">
        <rdfs:subClassOf rdf:resource="#User"/>
        <rdfs:comment>Represents the settings that can be edited in a user's profile, including profile picture, username, and personal details.</rdfs:comment>
    </owl:Class>

    <!-- Object Property: canEditProfile -->
    <owl:ObjectProperty rdf:about="#canEditProfile">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ProfileSettings"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that the user can edit their general profile settings.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: canEditProfilePicture -->
    <owl:ObjectProperty rdf:about="#canEditProfilePicture">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ProfileSettings"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Indicates that the user can edit their profile picture within the profile settings.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: canEditUsername -->
    <owl:ObjectProperty rdf:about="#canEditUsername">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#ProfileSettings"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Indicates that the user can edit their username within the profile settings.</rdfs:comment>
    </owl:ObjectProperty>

<!-- Class: Channel -->
    <owl:Class rdf:about="#Channel">
        <rdfs:comment>Represents a content creator's channel on the platform, with attributes and relationships to content and users.</rdfs:comment>
    </owl:Class>

    <!-- Data Property: hasChannelID -->
    <owl:DatatypeProperty rdf:about="#hasChannelID">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Unique identifier for the channel.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasChannelName -->
    <owl:DatatypeProperty rdf:about="#hasChannelName">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Name of the channel.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasChannelDescription -->
    <owl:DatatypeProperty rdf:about="#hasChannelDescription">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Description of the channel.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasChannelCreationDate -->
    <owl:DatatypeProperty rdf:about="#hasChannelCreationDate">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Date when the channel was created.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasChannelProfilePicture -->
    <owl:DatatypeProperty rdf:about="#hasChannelProfilePicture">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>URL of the channel's profile picture.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasSubscriberCount -->
    <owl:DatatypeProperty rdf:about="#hasSubscriberCount">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Total number of subscribers for the channel.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPlaylist-->
    <owl:DatatypeProperty rdf:about="#hasPlaylist">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>the channel to playlists</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasTotalViews -->
    <owl:DatatypeProperty rdf:about="#hasTotalViews">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Total number of views across all content in the channel.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Object Property: associatedWithUser -->
    <owl:ObjectProperty rdf:about="#associatedWithUser">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Links the channel to the user who owns or manages it.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: uploadsContent -->
    <owl:ObjectProperty rdf:about="#uploadsContent">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>General property linking the channel to uploaded content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: uploadsVideo -->
    <owl:ObjectProperty rdf:about="#uploadsVideo">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Video"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Links the channel to uploaded video content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: uploadsShortVideo -->
    <owl:ObjectProperty rdf:about="#uploadsShortVideo">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#ShortVideo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Links the channel to uploaded short videos.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: uploadsLive -->
    <owl:ObjectProperty rdf:about="#uploadsLive">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Live"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Links the channel to live sessions conducted through it.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: associatedWithMonetization -->
    <owl:ObjectProperty rdf:about="#associatedWithMonetization">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Monetization"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Links the channel to monetization settings or features.</rdfs:comment>
    </owl:ObjectProperty>

<!-- Class: ContentInteraction -->
    <owl:Class rdf:about="#ContentInteraction">
        <rdfs:comment>Represents interactions on content within the platform.</rdfs:comment>
    </owl:Class>

    <!-- Sub-class: InteractionType -->
    <owl:Class rdf:about="#InteractionType">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents different types of interactions with content.</rdfs:comment>
    </owl:Class>

    <!-- Sub-class: Like -->
    <owl:Class rdf:about="#Like">
        <rdfs:subClassOf rdf:resource="#InteractionType"/>
        <rdfs:comment>Represents a like interaction with content.</rdfs:comment>
    </owl:Class>


    <!-- Object Properties for Like -->
    <owl:ObjectProperty rdf:about="#likesContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user likes a specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#likesContent">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a channel likes a specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Indicates that a like is associated with a specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithLikedPost">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Indicates that a like is associated with a liked post.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithLikedVideo">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="#Video"/>
        <rdfs:comment>Indicates that a like is associated with a liked video.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithLikedShortVideo">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="#ShortVideo"/>
        <rdfs:comment>Indicates that a like is associated with a liked short video.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithLikedLive">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="#Live"/>
        <rdfs:comment>Indicates that a like is associated with a liked live session.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Property for Like -->
    <owl:DatatypeProperty rdf:about="#hasLikeTimestamp">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Records the timestamp when a like interaction occurred.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Sub-class: Comment -->
    <owl:Class rdf:about="#Comment">
        <rdfs:subClassOf rdf:resource="#InteractionType"/>
        <rdfs:comment>Represents a comment interaction with content.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for Comment -->
    <owl:ObjectProperty rdf:about="#commentsOnContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user comments on specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#commentsOnContent">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a channel comments on specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: commentedBy -->
    <owl:ObjectProperty rdf:about="#commentedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Links a comment with the User who made the comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#User"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Indicates that a comment is associated with specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithCommentedPost">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Indicates that a comment is associated with a commented post.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithCommentedVideo">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#Video"/>
        <rdfs:comment>Indicates that a comment is associated with a commented video.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithCommentedShortVideo">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#ShortVideo"/>
        <rdfs:comment>Indicates that a comment is associated with a commented short video.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithCommentedLive">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#Live"/>
        <rdfs:comment>Indicates that a comment is associated with a commented live session.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Properties for Comment -->
    <owl:DatatypeProperty rdf:about="#hasCommentText">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the text of a comment.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasCommentTimestamp">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Records the timestamp when a comment interaction occurred.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasDeviceInfo -->
    <owl:DatatypeProperty rdf:about="#hasDeviceInfo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the device information of the user device who gave a comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasUserInteractionIP -->
    <owl:DatatypeProperty rdf:about="#hasUserInteractionIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP information of the user device who gave a comment.</rdfs:comment>
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Sub-class: Share -->
    <owl:Class rdf:about="#Share">
        <rdfs:subClassOf rdf:resource="#InteractionType"/>
        <rdfs:comment>Represents a share interaction with content.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for Share -->
    <owl:ObjectProperty rdf:about="#sharesContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user shares specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sharesContent">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a channel shares specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Share"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Indicates that a share is associated with specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithSharedPost">
        <rdfs:domain rdf:resource="#Share"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Indicates that a share is associated with a shared post.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithSharedVideo">
        <rdfs:domain rdf:resource="#Share"/>
        <rdfs:range rdf:resource="#Video"/>
        <rdfs:comment>Indicates that a share is associated with a shared video.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithSharedShortVideo">
        <rdfs:domain rdf:resource="#Share"/>
        <rdfs:range rdf:resource="#ShortVideo"/>
        <rdfs:comment>Indicates that a share is associated with a shared short video.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithSharedLive">
        <rdfs:domain rdf:resource="#Share"/>
        <rdfs:range rdf:resource="#Live"/>
        <rdfs:comment>Indicates that a share is associated with a shared live session.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Property for Share -->
    <owl:DatatypeProperty rdf:about="#hasShareTimestamp">
        <rdfs:domain rdf:resource="#Share"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
