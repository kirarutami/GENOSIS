<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- Class: NewsInformation -->
    <owl:Class rdf:about="#NewsInformation">
        <rdfs:comment>Represents functionalities and components related to news information in the system.</rdfs:comment>
    </owl:Class>

    <!-- Sub-class: NewsWatchPage -->
    <owl:Class rdf:about="#NewsWatchPage">
        <rdfs:subClassOf rdf:resource="#NewsInformation"/>
        <rdfs:comment>Represents the page where users watch or read news content.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for NewsWatchPage -->
    <owl:ObjectProperty rdf:about="#viewsNewsWatchPage">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#NewsWatchPage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user views the news watch page.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Properties for NewsWatchPage -->
    <owl:DatatypeProperty rdf:about="#hasNewsPageContentType">
        <rdfs:domain rdf:resource="#NewsWatchPage"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the type of content displayed on the news watch page (e.g., video, article).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Sub-class: BreakingNews -->
    <owl:Class rdf:about="#BreakingNews">
        <rdfs:subClassOf rdf:resource="#NewsInformation"/>
        <rdfs:comment>Represents the breaking news section in the system.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for BreakingNews -->
    <owl:ObjectProperty rdf:about="#viewsBreakingNews">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#BreakingNews"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user views the breaking news section.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Properties for BreakingNews -->
    <owl:DatatypeProperty rdf:about="#hasBreakingNewsPriority">
        <rdfs:domain rdf:resource="#BreakingNews"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the priority level of breaking news (e.g., High, Medium, Low).</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Sub-class: TopNews -->
    <owl:Class rdf:about="#TopNews">
        <rdfs:subClassOf rdf:resource="#NewsInformation"/>
        <rdfs:comment>Represents the top news section in the system.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for TopNews -->
    <owl:ObjectProperty rdf:about="#viewsTopNews">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#TopNews"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user views the top news section.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Properties for TopNews -->
    <owl:DatatypeProperty rdf:about="#hasTopNewsAlgorithm">
        <rdfs:domain rdf:resource="#TopNews"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the algorithm used to determine the top news.</rdfs:comment>
    </owl:DatatypeProperty>

</rdf:RDF>
