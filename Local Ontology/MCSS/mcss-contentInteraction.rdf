    </owl:DatatypeProperty>

    <!-- Sub-class: WatchLater -->
    <owl:Class rdf:about="#WatchLater">
        <rdfs:subClassOf rdf:resource="#InteractionType"/>
        <rdfs:comment>Represents the watch later feature for videos.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for WatchLater -->
    <owl:ObjectProperty rdf:about="#addsVideoToWatchLater">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Video"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user adds a video to their watch later list.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithVideo">
        <rdfs:domain rdf:resource="#WatchLater"/>
        <rdfs:range rdf:resource="#Video"/>
        <rdfs:comment>Indicates that a watch later action is associated with a specific video.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Property for WatchLater -->
    <owl:DatatypeProperty rdf:about="#hasWatchLaterTimestamp">
        <rdfs:domain rdf:resource="#WatchLater"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Records the timestamp when a video was added to the watch later list.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Sub-class: Gift -->
    <owl:Class rdf:about="#Gift">
        <rdfs:subClassOf rdf:resource="#InteractionType"/>
        <rdfs:comment>Represents gifting interactions during live sessions.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for Gift -->
    <owl:ObjectProperty rdf:about="#sendsGiftInLive">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Live"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user sends a gift during a live session.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sendsGiftInLive">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Live"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a channel sends a gift during a live session.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#receivesGiftInLive">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Live"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user receive a gift during a live session.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithLive">
        <rdfs:domain rdf:resource="#Gift"/>
        <rdfs:range rdf:resource="#Live"/>
        <rdfs:comment>Indicates that a gift interaction is associated with a specific live session.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Properties for Gift -->
    <owl:DatatypeProperty rdf:about="#hasGiftComment">
        <rdfs:domain rdf:resource="#Gift"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores a comment provided with the gift.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGiftPrice">
        <rdfs:domain rdf:resource="#Gift"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the price of the gift sent during a live session.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGiftTimestamp">
        <rdfs:domain rdf:resource="#Gift"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Records the timestamp when a gift was sent during a live session.</rdfs:comment>
    </owl:DatatypeProperty>

</rdf:RDF>


