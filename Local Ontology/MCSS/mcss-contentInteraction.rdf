        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a channel comments on specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Indicates that a comment is associated with specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithCommentedPost">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Indicates that a comment is associated with a commented post.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithCommentedVideo">
        <rdfs:domain rdf:resource="#Comment"/>
