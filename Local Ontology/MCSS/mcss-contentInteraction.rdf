    <!-- Sub-class: Report -->
    <owl:Class rdf:about="#Report">
        <rdfs:subClassOf rdf:resource="#InteractionType"/>
        <rdfs:comment>Represents a report interaction with content.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for Report -->
    <owl:ObjectProperty rdf:about="#reportsContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user reports specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#reportsContent">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a channel reports specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#reportsChannel">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Channel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user reports specific channel.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#reportsUser">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user reports specific user.</rdfs:comment>
    </owl:ObjectProperty>


    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Report"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Indicates that a report is associated with specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithReportedPost">
        <rdfs:domain rdf:resource="#Report"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Indicates that a report is associated with a reported post.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithReportedVideo">
        <rdfs:domain rdf:resource="#Report"/>
        <rdfs:range rdf:resource="#Video"/>
        <rdfs:comment>Indicates that a report is associated with a reported video.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithReportedShortVideo">
        <rdfs:domain rdf:resource="#Report"/>
        <rdfs:range rdf:resource="#ShortVideo"/>
        <rdfs:comment>Indicates that a report is associated with a reported short video.</rdfs:comment>
    </owl:ObjectProperty>

