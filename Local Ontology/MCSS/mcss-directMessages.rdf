<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- Class: DirectMessages -->
    <owl:Class rdf:about="#DirectMessages">
        <rdfs:comment>Represents the functionality and content of direct messages in the system.</rdfs:comment>
    </owl:Class>

    <!-- Sub-class: MessageContent -->
    <owl:Class rdf:about="#MessageContent">
        <rdfs:subClassOf rdf:resource="#DirectMessages"/>
        <rdfs:comment>Represents the content of a message, including text, media, and metadata.</rdfs:comment>
    </owl:Class>

    <!-- Data Properties for MessageContent -->
    <owl:DatatypeProperty rdf:about="#hasTextContent">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the text content of a message.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasMediaAttachment">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment>Represents the URL of a media attachment in the message.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasTimestamp">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the timestamp of when the message was sent.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasMessageStatus">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Represents the status of the message, such as sent, delivered, or read.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasMessageReaction">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Represents an array of emojis reacting to the message.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#isMessageFlagged">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the message has been flagged as crime.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#isMessageEncrypted">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the message is encrypted.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Sub-class: MessageActions -->
    <owl:Class rdf:about="#MessageActions">
        <rdfs:subClassOf rdf:resource="#DirectMessages"/>
        <rdfs:comment>Represents user actions performed on messages, such as sending or deleting.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for MessageActions -->
    <owl:ObjectProperty rdf:about="#sendsMessageTo">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Represents a user sending a message to another user.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sendsMediaMessageTo">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Represents a user sending a media message to another user.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sendsVoiceNoteTo">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Represents a user sending a voice note to another user.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#reactsToMessage">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdfs:comment>Represents a user reacting to a message with an emoji or other reaction.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#repliesToMessage">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Represents a user replying to a specific message.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#deletesMessage">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents a user deleting a specific message.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#reportsMessage">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Represents a user reporting a message for review.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#forwardsMessageTo">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Represents a user forwarding a message to another user.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#marksMessageAsUnread">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents a user marking a message as unread.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#archivesMessage">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents a user archiving a specific message.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Sub-class: AdvancedMessagingFeatures -->
    <owl:Class rdf:about="#AdvancedMessagingFeatures">
        <rdfs:subClassOf rdf:resource="#DirectMessages"/>
        <rdfs:comment>Represents advanced features for messaging, such as encryption and priority settings.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for AdvancedMessagingFeatures -->
    <owl:ObjectProperty rdf:about="#setsMessagePriority">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Allows a user to set priority for a specific message.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#enablesMessageEncryptionFor">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Enables encryption for messages sent to a specific user.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sendsGiftInMessageTo">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Allows a user to send a gift within a message to another user.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#schedulesMessageFor">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Schedules a message to be sent at a later time.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#recallsMessage">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Recalls a previously sent message.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#translatesMessageTo">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Allows a user to translate a message into another language.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#pinsMessageInChat">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Allows a user to pin a specific message in a chat.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Sub-class: NotificationSettings -->
    <owl:Class rdf:about="#NotificationSettings">
        <rdfs:subClassOf rdf:resource="#DirectMessages"/>
        <rdfs:comment>Represents settings for managing message notifications.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for NotificationSettings -->
    <owl:ObjectProperty rdf:about="#mutesMessageNotificationsFrom">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Mutes notifications from a specific user.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#enablesReadReceiptsFor">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Enables read receipts for messages sent to a specific user.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#disablesReadReceiptsFor">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Disables read receipts for messages sent to a specific user.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Properties for NotificationSettings -->
    <owl:DatatypeProperty rdf:about="#hasUnreadMessageCount">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the number of unread messages for a user.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasLastMessagePreview">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Displays a preview of the most recent message.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasNotificationSoundSetting">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the user's chosen notification sound setting.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#enablesNotificationForGroupMessages">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#GroupMessage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Enables notifications for group messages.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Class: GroupMessages -->
    <owl:Class rdf:about="#GroupMessage">
        <rdfs:subClassOf rdf:resource="#DirectMessages"/>
        <rdfs:comment>Represents group message threads and related interactions.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for GroupMessages -->
    <owl:ObjectProperty rdf:about="#createsGroupMessageThreadWith">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#GroupMessage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents a user creating a group message thread with participants.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasGroupParticipants">
        <rdfs:domain rdf:resource="#GroupMessage"/>
        <rdfs:range rdf:resource="#User"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment>Represents participants in a group message thread.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#leavesGroupMessageThread">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#GroupMessage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Represents a user leaving a group message thread.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#addsParticipantsToGroup">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#GroupMessage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Represents a user adding participants to a group message thread.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#removesParticipantsFromGroup">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#GroupMessage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:comment>Represents a user removing participants from a group message thread.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Properties for GroupMessages -->
    <owl:DatatypeProperty rdf:about="#hasGroupMessageTitle">
        <rdfs:domain rdf:resource="#GroupMessage"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the title of a group message thread.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGroupMessageCreatedTimestamp">
        <rdfs:domain rdf:resource="#GroupMessage"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the timestamp when the group message thread was created.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGroupMessageLastActivity">
        <rdfs:domain rdf:resource="#GroupMessage"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the timestamp of the last activity in the group message thread.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Class: MessageAnalytics -->
    <owl:Class rdf:about="#MessageAnalytics">
        <rdfs:subClassOf rdf:resource="#DirectMessages"/>
        <rdfs:comment>Represents analytics related to messages, such as open rate and response time.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for MessageAnalytics -->
    <owl:ObjectProperty rdf:about="#viewsMessageStats">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#MessageContent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents a user viewing analytics for a specific message.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Properties for MessageAnalytics -->
    <owl:DatatypeProperty rdf:about="#hasOpenRate">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the open rate of a message in percentage.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasClickThroughRate">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the click-through rate of a message in percentage.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasResponseTime">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Represents the average response time for a message in seconds.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#isMessageBoosted">
        <rdfs:domain rdf:resource="#MessageContent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether a message has been boosted for visibility.</rdfs:comment>
    </owl:DatatypeProperty>

</rdf:RDF>


