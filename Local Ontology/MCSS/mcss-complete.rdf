        <rdfs:comment>Records the timestamp when a share interaction occurred.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Sub-class: Dislike -->
    <owl:Class rdf:about="#Dislike">
        <rdfs:subClassOf rdf:resource="#InteractionType"/>
        <rdfs:comment>Represents a dislike interaction with content.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for Dislike -->
    <owl:ObjectProperty rdf:about="#dislikesContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a user dislikes specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#dislikesContent">
        <rdfs:domain rdf:resource="#Channel"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates that a channel dislikes specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Dislike"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Indicates that a dislike is associated with specific content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithDislikedPost">
        <rdfs:domain rdf:resource="#Dislike"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Indicates that a dislike is associated with a disliked post.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithDislikedVideo">
