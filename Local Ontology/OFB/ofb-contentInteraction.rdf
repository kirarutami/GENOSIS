<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- ContentInteraction Class -->
    <owl:Class rdf:about="#ContentInteraction">
        <rdfs:comment>Represents all forms of user interactions with content, including voting, liking, and replying.</rdfs:comment>
    </owl:Class>

    <!-- Engagement Sub-Class -->
    <owl:Class rdf:about="#Engagement">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents various types of engagement actions performed on content.</rdfs:comment>
    </owl:Class>

    <!-- UpvoteDownvote Sub-Class -->
    <owl:Class rdf:about="#UpvoteDownvote">
        <rdfs:subClassOf rdf:resource="#Engagement"/>
        <rdfs:comment>Represents upvote or downvote actions on content.</rdfs:comment>
    </owl:Class>

    <!-- Properties for UpvoteDownvote -->
    <owl:ObjectProperty rdf:about="#upvotesContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a user to the content they have upvoted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#downvotesContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a user to the content they have downvoted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasUpvoteTimestamp">
        <rdfs:domain rdf:resource="#UpvoteDownvote"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when an upvote was made.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasDownvoteTimestamp">
        <rdfs:domain rdf:resource="#UpvoteDownvote"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a downvote was made.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#UpvoteDownvote"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links an upvote or downvote action to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Like Sub-Class -->
    <owl:Class rdf:about="#Like">
        <rdfs:subClassOf rdf:resource="#Engagement"/>
        <rdfs:comment>Represents a like action on content.</rdfs:comment>
    </owl:Class>

    <!-- Properties for Like -->
    <owl:ObjectProperty rdf:about="#likesContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a user to the content they have liked.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasLikeTimestamp">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a like was made.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Like"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a like action to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- ReplyToComments Sub-Class -->
    <owl:Class rdf:about="#ReplyToComments">
        <rdfs:subClassOf rdf:resource="#Engagement"/>
        <rdfs:comment>Represents a reply to a comment action.</rdfs:comment>
    </owl:Class>

    <!-- Properties for ReplyToComments -->
    <owl:ObjectProperty rdf:about="#repliesToComment">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Comment"/>
        <rdfs:comment>Links a user to the comment they have replied to.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasReplyText">
        <rdfs:domain rdf:resource="#ReplyToComments"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Stores the text content of a reply.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasReplyTimestamp">
        <rdfs:domain rdf:resource="#ReplyToComments"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a reply was made.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#associatedWithComment">
        <rdfs:domain rdf:resource="#ReplyToComments"/>
        <rdfs:range rdf:resource="#Comment"/>
        <rdfs:comment>Links a reply action to the associated comment.</rdfs:comment>
    </owl:ObjectProperty>


    <!-- ShareRepost Sub-Class -->
    <owl:Class rdf:about="#ShareRepost">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents sharing or reposting content to a community or other platforms.</rdfs:comment>
    </owl:Class>

    <!-- Properties for ShareRepost -->
    <owl:ObjectProperty rdf:about="#sharesContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a user to the content they have shared or reposted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasShareTimestamp">
        <rdfs:domain rdf:resource="#ShareRepost"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when content was shared.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#sharedToCommunity">
        <rdfs:domain rdf:resource="#ShareRepost"/>
        <rdfs:range rdf:resource="#Community"/>
        <rdfs:comment>Links shared content to the community where it was reposted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#ShareRepost"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a share or repost action to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Bookmark Sub-Class -->
    <owl:Class rdf:about="#Bookmark">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents bookmarking content for later reference.</rdfs:comment>
    </owl:Class>

    <!-- Properties for Bookmark -->
    <owl:ObjectProperty rdf:about="#bookmarksContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a user to the content they have bookmarked.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasBookmarkTimestamp">
        <rdfs:domain rdf:resource="#Bookmark"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when content was bookmarked.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Bookmark"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a bookmark action to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- GiveAwards Sub-Class -->
    <owl:Class rdf:about="#GiveAwards">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents giving awards to content or users as recognition.</rdfs:comment>
    </owl:Class>

    <!-- Properties for GiveAwards -->
    <owl:ObjectProperty rdf:about="#givesAwardToContent">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a user to the content they have awarded.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasAwardType">
        <rdfs:domain rdf:resource="#GiveAwards"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the type of award given (e.g., Gold, Silver, Platinum).</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasAwardTimestamp">
        <rdfs:domain rdf:resource="#GiveAwards"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when an award was given.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#GiveAwards"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links an award action to the associated content.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWithAwardingUser">
        <rdfs:domain rdf:resource="#GiveAwards"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Links an award action to the user who awarded the content.</rdfs:comment>
    </owl:ObjectProperty>


    <!-- BlockUnblockUser Sub-Class -->
    <owl:Class rdf:about="#BlockUnblockUser">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents blocking and unblocking actions between users.</rdfs:comment>
    </owl:Class>

    <!-- Properties for BlockUnblockUser -->
    <owl:ObjectProperty rdf:about="#blocksUser">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Links a user to another user they have blocked.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#unblocksUser">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Links a user to another user they have unblocked.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasBlockReason">
        <rdfs:domain rdf:resource="#BlockUnblockUser"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Stores the reason for blocking a user.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasBlockTimestamp">
        <rdfs:domain rdf:resource="#BlockUnblockUser"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a user was blocked.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasUnblockTimestamp">
        <rdfs:domain rdf:resource="#BlockUnblockUser"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a user was unblocked.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Mute Sub-Class -->
    <owl:Class rdf:about="#Mute">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents muting actions on users or topics.</rdfs:comment>
    </owl:Class>

    <!-- Topic Sub-Class -->
    <owl:Class rdf:about="#Topic">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents topics on forums</rdfs:comment>
    </owl:Class>

    <!-- Properties for Mute -->
    <owl:ObjectProperty rdf:about="#mutesUser">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#User"/>
        <rdfs:comment>Links a user to another user they have muted.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#mutesTopic">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Topic"/>
        <rdfs:comment>Links a user to a topic they have muted.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasMuteTimestamp">
        <rdfs:domain rdf:resource="#Mute"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a topic or user was muted.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#associatedWithTopic">
        <rdfs:domain rdf:resource="#Mute"/>
        <rdfs:range rdf:resource="#Topic"/>
        <rdfs:comment>Links a muting action to the associated topic.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- FollowTopics Sub-Class -->
    <owl:Class rdf:about="#FollowTopics">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents following topics actions.</rdfs:comment>
    </owl:Class>

    <!-- Properties for FollowTopics -->
    <owl:ObjectProperty rdf:about="#followsTopic">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Topic"/>
        <rdfs:comment>Links a user to a topic they are following.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasFollowTimestamp">
        <rdfs:domain rdf:resource="#FollowTopics"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a user started following a topic.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#associatedWithTopic">
        <rdfs:domain rdf:resource="#FollowTopics"/>
        <rdfs:range rdf:resource="#Topic"/>
        <rdfs:comment>Links a following action to the associated topic.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- PinPosts Sub-Class -->
    <owl:Class rdf:about="#PinPosts">
        <rdfs:subClassOf rdf:resource="#ContentInteraction"/>
        <rdfs:comment>Represents pinning actions for posts by users or communities.</rdfs:comment>
    </owl:Class>

    <!-- Properties for PinPosts -->
    <owl:ObjectProperty rdf:about="#pinsPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Links a user to a post they have pinned.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#pinsPostByCommunity">
        <rdfs:domain rdf:resource="#Community"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Links a community to a post it has pinned.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="#hasPinTimestamp">
        <rdfs:domain rdf:resource="#PinPosts"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a post was pinned.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="#associatedWithPost">
        <rdfs:domain rdf:resource="#PinPosts"/>
        <rdfs:range rdf:resource="#Post"/>
        <rdfs:comment>Links a pinning action to the associated post.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Comment Sub-Class -->
    <owl:Class rdf:about="#Comment">
        <rdfs:subClassOf rdf:resource="#Engagement"/>
        <rdfs:comment>Represents comments made by users on content.</rdfs:comment>
    </owl:Class>

    <!-- Object Property: User -> commentsOn -> Content -->
    <owl:ObjectProperty rdf:about="#commentsOn">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a user to the content they have commented on.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

    <!-- Data Property: hasCommentText -->
    <owl:DatatypeProperty rdf:about="#hasCommentText">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Stores the text of a comment.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasCommentTimestamp -->
    <owl:DatatypeProperty rdf:about="#hasCommentTimestamp">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Records the timestamp when a comment was made.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Object Property: associatedWithContent -->
    <owl:ObjectProperty rdf:about="#associatedWithContent">
        <rdfs:domain rdf:resource="#Comment"/>
        <rdfs:range rdf:resource="#Content"/>
        <rdfs:comment>Links a comment to the content it is associated with.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>

</rdf:RDF>


