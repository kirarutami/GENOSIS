        <owl:disjointWith rdf:resource="#Analytics"/>
        <owl:disjointWith rdf:resource="#Accessibility"/>
    </owl:Class>

    <owl:Class rdf:about="#Notifications">
        <owl:disjointWith rdf:resource="#Messaging"/>
        <owl:disjointWith rdf:resource="#Monetization"/>
        <owl:disjointWith rdf:resource="#Analytics"/>
        <owl:disjointWith rdf:resource="#Accessibility"/>
    </owl:Class>

    <owl:Class rdf:about="#Messaging">
        <owl:disjointWith rdf:resource="#Monetization"/>
        <owl:disjointWith rdf:resource="#Analytics"/>
        <owl:disjointWith rdf:resource="#Accessibility"/>
    </owl:Class>

    <owl:Class rdf:about="#Monetization">
        <owl:disjointWith rdf:resource="#Analytics"/>
        <owl:disjointWith rdf:resource="#Accessibility"/>
    </owl:Class>

    <owl:Class rdf:about="#Analytics">
        <owl:disjointWith rdf:resource="#Accessibility"/>
    </owl:Class>

    <!-- Disjointness for Sub-Classes -->
    <owl:Class rdf:about="#ProfileSettings">
        <owl:disjointWith rdf:resource="#SecuritySettings"/>
        <owl:disjointWith rdf:resource="#SubscriptionPlan"/>
        <owl:disjointWith rdf:resource="#NotificationSettings"/>
        <owl:disjointWith rdf:resource="#AnonymousSession"/>
        <owl:disjointWith rdf:resource="#AccountDeactivation"/>
        <owl:disjointWith rdf:resource="#AccountDeletion"/>
        <owl:disjointWith rdf:resource="#Session"/>
    </owl:Class>

    <owl:Class rdf:about="#ContentInteraction">
        <owl:disjointWith rdf:resource="#Engagement"/>
    </owl:Class>

    <owl:Class rdf:about="#Engagement">
        <owl:disjointWith rdf:resource="#UpvoteDownvote"/>
        <owl:disjointWith rdf:resource="#Like"/>
        <owl:disjointWith rdf:resource="#ReplyToComments"/>
        <owl:disjointWith rdf:resource="#ShareRepost"/>
    </owl:Class>

    <owl:Class rdf:about="#SearchFeatures">
        <owl:disjointWith rdf:resource="#Recommendations"/>
    </owl:Class>

    <owl:Class rdf:about="#Recommendations">
