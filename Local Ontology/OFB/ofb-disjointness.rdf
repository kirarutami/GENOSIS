    </owl:Class>

    <owl:Class rdf:about="#AnonymousSession">
        <owl:disjointWith rdf:resource="#AccountDeletion"/>
        <owl:disjointWith rdf:resource="#Session"/>
    </owl:Class>

    <!-- Disjointness for Community Moderation Sub-classes -->
    <owl:Class rdf:about="#Moderator">
        <owl:disjointWith rdf:resource="#ModerationQueue"/>
        <owl:disjointWith rdf:resource="#CommunityRules"/>
    </owl:Class>

    <owl:Class rdf:about="#ModerationQueue">
        <owl:disjointWith rdf:resource="#CommunityRules"/>
    </owl:Class>

    <!-- Disjointness for ContentInteraction Engagement Sub-classes -->
    <owl:Class rdf:about="#UpvoteDownvote">
        <owl:disjointWith rdf:resource="#Like"/>
        <owl:disjointWith rdf:resource="#ReplyToComments"/>
        <owl:disjointWith rdf:resource="#ShareRepost"/>
    </owl:Class>

    <owl:Class rdf:about="#Like">
        <owl:disjointWith rdf:resource="#ReplyToComments"/>
        <owl:disjointWith rdf:resource="#ShareRepost"/>
        <owl:disjointWith rdf:resource="#Bookmark"/>
    </owl:Class>

    <owl:Class rdf:about="#ReplyToComments">
        <owl:disjointWith rdf:resource="#ShareRepost"/>
        <owl:disjointWith rdf:resource="#Bookmark"/>
    </owl:Class>

    <!-- Disjointness for Search and Recommendations -->
    <owl:Class rdf:about="#GlobalSearch">
        <owl:disjointWith rdf:resource="#AdvancedSearch"/>
