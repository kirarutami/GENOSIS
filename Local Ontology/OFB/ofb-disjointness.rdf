    </owl:Class>

    <!-- Disjointness for Analytics Sub-classes -->
    <owl:Class rdf:about="#EngagementMetrics">
        <owl:disjointWith rdf:resource="#CommunityMetrics"/>
    </owl:Class>

    <owl:Class rdf:about="#CommunityMetrics">
        <owl:disjointWith rdf:resource="#UserInsights"/>
    </owl:Class>

    <!-- Disjointness for Notifications Sub-classes -->
    <owl:Class rdf:about="#PushNotifications">
        <owl:disjointWith rdf:resource="#EmailNotifications"/>
        <owl:disjointWith rdf:resource="#InAppNotifications"/>
    </owl:Class>

    <owl:Class rdf:about="#ActivitySummaries">
        <owl:disjointWith rdf:resource="#WeeklyDigests"/>
    </owl:Class>

