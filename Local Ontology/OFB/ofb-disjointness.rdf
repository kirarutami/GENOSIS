    </owl:Class>

    <owl:Class rdf:about="#AdvancedSearch">
        <owl:disjointWith rdf:resource="#TrendSearch"/>
        <owl:disjointWith rdf:resource="#PersonalizedFeed"/>
    </owl:Class>

    <owl:Class rdf:about="#PersonalizedFeed">
