    <owl:ObjectProperty rdf:about="#shareListing">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#TodaysPicks"/>
        <rdfs:comment>Links a user to the action of sharing a listing.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Sub-kelas SellerInformation -->
    <owl:Class rdf:about="#SellerInformation">
        <rdfs:subClassOf rdf:resource="#SellingPage"/>
