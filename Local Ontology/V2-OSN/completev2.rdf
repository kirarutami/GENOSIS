    </owl:ObjectProperty>

    <!-- User adds text comment to post -->
    <owl:ObjectProperty rdf:about="#addsTextCommentToPost">
        <rdfs:domain rdf:resource="#User"/>
        <rdfs:range rdf:resource="#TextComment"/>
