    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGroupDescription">
        <rdfs:domain rdf:resource="#GroupManagement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Represents the group description of a managed group.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasGroupPrivacy">
        <rdfs:domain rdf:resource="#GroupManagement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Represents the privacy settings of a managed group.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Sub-kelas GroupFeatures -->
    <owl:Class rdf:about="#GroupFeatures">
        <rdfs:subClassOf rdf:resource="#GroupManagement"/>
        <rdfs:comment>Features available in groups such as events, files, and announcements.</rdfs:comment>
    </owl:Class>

