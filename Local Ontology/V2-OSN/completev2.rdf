        <rdfs:comment>Represents a GIF comment on a post or other content.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#EmojiComment">
        <rdfs:subClassOf rdf:resource="#Comments"/>
        <rdfs:comment>Represents an emoji comment on a post or other content.</rdfs:comment>
    </owl:Class>

    <!-- Object Properties for ContentInteraction -->

    <!-- User likes post -->
    <owl:ObjectProperty rdf:about="#likesPost">
        <rdfs:domain rdf:resource="#User"/>
