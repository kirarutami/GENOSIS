    </owl:Class>

    <!-- Individu di dalam AccountSettings -->
    <owl:NamedIndividual rdf:about="#GeneralSettings">
        <rdf:type rdf:resource="#AccountSettings"/>
        <hasFeatureDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">User can manage general account settings.</hasFeatureDescription>
    </owl:NamedIndividual>

