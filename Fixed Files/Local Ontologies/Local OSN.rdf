<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    

<!-- Kelas: OnlineSocialNetworkSites -->
    <owl:Class rdf:about="#OnlineSocialNetworkSites">
        <rdfs:comment>Represents various online social network platforms, such as Facebook, Instagram, LinkedIn, etc.</rdfs:comment>
    </owl:Class>

    <!-- Individual: Facebook -->
    <owl:NamedIndividual rdf:about="#Facebook">
        <rdf:type rdf:resource="#OnlineSocialNetworkSites"/>
        <rdfs:comment>Represents the Facebook social media platform.</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Individual: Instagram -->
    <owl:NamedIndividual rdf:about="#Instagram">
        <rdf:type rdf:resource="#OnlineSocialNetworkSites"/>
        <rdfs:comment>Represents the Instagram social media platform.</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Individual: LinkedIn -->
    <owl:NamedIndividual rdf:about="#LinkedIn">
        <rdf:type rdf:resource="#OnlineSocialNetworkSites"/>
        <rdfs:comment>Represents the LinkedIn social media platform.</rdfs:comment>
    </owl:NamedIndividual>

<!-- Class: User -->
    <owl:Class rdf:about="#User">
        <rdfs:comment>The primary class representing the user in the system.</rdfs:comment>
        <owl:disjointWith rdf:resource="#SecuritySettings"/>
        <owl:disjointWith rdf:resource="#PrivacySettings"/>
    </owl:Class>

    

    <!-- Class: UserProfile -->
    <owl:Class rdf:about="#UserProfile">
        <rdfs:comment>Represents the profile of a user, containing personal information such as profile picture, username, and email.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#User"/>
        <owl:disjointWith rdf:resource="#SecuritySettings"/>
        <owl:disjointWith rdf:resource="#PrivacySettings"/>
    </owl:Class>

    <!-- Object Property: hasProfileOn -->
    <owl:ObjectProperty rdf:about="#hasProfileOn">
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="#OnlineSocialNetworkSites"/>
        <rdfs:comment>Indicates that a UserProfile exists on a specific Online Social Network Site (e.g., Facebook, Instagram, LinkedIn).</rdfs:comment>
        <!-- <owl:inverseOf rdf:resource="#belongsTo"/> -->
    </owl:ObjectProperty>

    <!-- Object Property: belongsTo -->
    <owl:ObjectProperty rdf:about="#belongsTo">
        <rdfs:domain rdf:resource="#OnlineSocialNetworkSites"/>
        <rdfs:range rdf:resource="#UserProfile"/>
        <rdfs:comment>The inverse property of 'hasProfileOn', indicating that a social media profile belongs to a user on a given platform.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: isLinkedTo -->
    <owl:ObjectProperty rdf:about="#isLinkedTo">
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="#UserProfile"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment>Indicates that a UserProfile on one platform is linked to another profile on a different platform. This property is symmetric.</rdfs:comment>
    </owl:ObjectProperty>

     <!-- Definition of follows -->
    <owl:ObjectProperty rdf:about="#follows">
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="#UserProfile"/>
    </owl:ObjectProperty>

    <!-- Definition of isFollowedBy -->
    <owl:ObjectProperty rdf:about="#isFollowedBy">
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="#UserProfile"/>
        <owl:inverseOf rdf:resource="#follows"/>
    </owl:ObjectProperty>

    <!-- Definition of isFriendOf -->
    <owl:ObjectProperty rdf:about="#isFriendOf">
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="#UserProfile"/>
        <owl:SymmetricProperty rdf:resource="#isFriendOf"/>
    </owl:ObjectProperty>

    <!-- Properti Object: isMemberOf -->
    <owl:ObjectProperty rdf:about="#isMemberOf">
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="#Groups"/>
        <rdfs:comment>Indicates that a user is a member of a group.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Object Property: sameAs -->
    <owl:ObjectProperty rdf:about="#sameAs">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="#UserProfile"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment>Indicates that two profiles on different Online Social Network Sites belong to the same person. This property is functional and inverse functional.</rdfs:comment>
        <!-- <owl:disjointWith rdf:resource="#isLinkedTo"/> -->
    </owl:ObjectProperty>



        <!-- Object Property: User links to Online Social Network Sites -->
    <owl:ObjectProperty rdf:about="#linksAccount">
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="#OnlineSocialNetworkSites"/>
        <rdfs:comment>Links a user to their associated social network accounts.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Data Property: User has a linked account name -->
    <owl:DatatypeProperty rdf:about="#hasLinkedAccount">
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Stores the username or identifier of the linked social media account.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Data Property: hasFirstName -->
    <owl:DatatypeProperty rdf:about="#hasFirstName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the first name of the user.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <propertyDisjointWith rdf:resource="#hasLastName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLastName -->
    <owl:DatatypeProperty rdf:about="#hasLastName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the last name of the user.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <propertyDisjointWith rdf:resource="#hasFirstName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasMiddleName -->
    <owl:DatatypeProperty rdf:about="#hasMiddleName">
        <rdfs:comment>Stores the middle name of the user, if applicable.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <propertyDisjointWith rdf:resource="#hasFirstName"/>
        <propertyDisjointWith rdf:resource="#hasLastName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasProfilePicture -->
    <owl:DatatypeProperty rdf:about="#hasProfilePicture">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the URL or file path to the user's profile picture.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <propertyDisjointWith rdf:resource="#hasCoverPhoto"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasProfilePictureHash -->
    <owl:DatatypeProperty rdf:about="#hasProfilePictureHash">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>For verifying the integrity of the profile picture using hash.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <propertyDisjointWith rdf:resource="#hasCoverPhoto"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasCoverPhoto -->
    <owl:DatatypeProperty rdf:about="#hasCoverPhoto">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the URL or file path to the user's cover photo.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <propertyDisjointWith rdf:resource="#hasProfilePicture"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasUsername -->
    <owl:DatatypeProperty rdf:about="#hasUsername">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the unique username of the user.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <propertyDisjointWith rdf:resource="#hasEmail"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasEmail -->
    <owl:DatatypeProperty rdf:about="#hasEmail">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the email address of the user.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <propertyDisjointWith rdf:resource="#hasUsername"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasBio -->
    <owl:DatatypeProperty rdf:about="#hasBio">
        <rdfs:comment>Stores the biography or short description of the user.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasDetails -->
    <owl:DatatypeProperty rdf:about="#hasDetails">
        <rdfs:comment>Stores additional details about the user.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasHighlights -->
    <owl:DatatypeProperty rdf:about="#hasHighlights">
        <rdfs:comment>Stores highlights associated with the user's profile.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasAccountCreationDate -->
    <owl:DatatypeProperty rdf:about="#hasAccountCreationDate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the account creation date for the user.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasAccountVerification -->
    <owl:DatatypeProperty rdf:about="#hasAccountVerification">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the user's account has been verified.</rdfs:comment>
        <rdfs:domain rdf:resource="#UserProfile"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Class: PrivacySettings -->
    <owl:Class rdf:about="#PrivacySettings">
        <rdfs:comment>Represents the privacy settings related to the user's account, including blocked accounts and story settings.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#User"/>
        <owl:disjointWith rdf:resource="#UserProfile"/>
    </owl:Class>

    <!-- Data Property: hasAccountPrivacy -->
    <owl:DatatypeProperty rdf:about="#hasAccountPrivacy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the privacy setting of the user's account, indicating whether the account is private or public.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPrivateAccount -->
    <owl:DatatypeProperty rdf:about="#hasPrivateAccount">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the user account is private.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasCloseFriends -->
    <owl:DatatypeProperty rdf:about="#hasCloseFriends">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the user has a close friends list.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasBlockedAccounts -->
    <owl:DatatypeProperty rdf:about="#hasBlockedAccounts">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the user has blocked accounts.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasBlockingOptions -->
    <owl:DatatypeProperty rdf:about="#hasBlockingOptions">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the available blocking options for the user.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasBlockNotifications -->
    <owl:DatatypeProperty rdf:about="#hasBlockNotifications">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the user is notified about blocked accounts.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasReviewBlockedAccounts -->
    <owl:DatatypeProperty rdf:about="#hasReviewBlockedAccounts">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the user has reviewed the list of blocked accounts.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasHideStory -->
    <owl:DatatypeProperty rdf:about="#hasHideStory">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores whether the user's story is hidden from certain users.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasAllowStoryReplies -->
    <owl:DatatypeProperty rdf:about="#hasAllowStoryReplies">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether replies to the user's story are allowed.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasStoryExpiration -->
    <owl:DatatypeProperty rdf:about="#hasStoryExpiration">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the expiration time of the user's story.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLastStoryUpdate -->
    <owl:DatatypeProperty rdf:about="#hasLastStoryUpdate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the time of the last update to the user's story.</rdfs:comment>
        <rdfs:domain rdf:resource="#PrivacySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Class: SecuritySettings -->
    <owl:Class rdf:about="#SecuritySettings">
        <rdfs:comment>Represents the security settings of the user's account, including session management, two-factor authentication, and login alerts.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#User"/>
        <owl:disjointWith rdf:resource="#UserProfile"/>
    </owl:Class>

    <!-- Data Property: hasActiveSessions -->
    <owl:DatatypeProperty rdf:about="#hasActiveSessions">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the number of active sessions the user has.</rdfs:comment>
        <rdfs:domain rdf:resource="#SecuritySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLastLogin -->
    <owl:DatatypeProperty rdf:about="#hasLastLogin">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the date and time of the user's last login.</rdfs:comment>
        <rdfs:domain rdf:resource="#SecuritySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLoginIP -->
    <owl:DatatypeProperty rdf:about="#hasLoginIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the IP address used during the user's last login session.</rdfs:comment>
        <rdfs:domain rdf:resource="#SecuritySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasLogoutFromSessions -->
    <owl:DatatypeProperty rdf:about="#hasLogoutFromSessions">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates whether the user has logged out from all active sessions.</rdfs:comment>
        <rdfs:domain rdf:resource="#SecuritySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPhoneNumberVerificationStatus -->
    <owl:DatatypeProperty rdf:about="#hasPhoneNumberVerificationStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Indicates the verification status of the user's phone number.</rdfs:comment>
        <rdfs:domain rdf:resource="#SecuritySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPhoneNumber -->
    <owl:DatatypeProperty rdf:about="#hasPhoneNumber">
        <rdfs:comment>Stores the user's phone number.</rdfs:comment>
        <rdfs:domain rdf:resource="#SecuritySettings"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

<!-- Class: ContentCreation -->
    <owl:Class rdf:about="#ContentCreation">
        <rdfs:comment>The main class representing content creation functionality, including posts, stories, and reels.</rdfs:comment>
        <owl:disjointWith rdf:resource="#User"/>
        <owl:disjointWith rdf:resource="#Marketplace"/>
    </owl:Class>

    <!-- Class: Posts -->
    <owl:Class rdf:about="#Posts">
        <rdfs:comment>Represents posts created by users, including media posts, location check-ins, and live videos.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ContentCreation"/>
        <owl:disjointWith rdf:resource="#Stories"/>
        <owl:disjointWith rdf:resource="#Reels"/>
    </owl:Class>

    <!-- Sub-class: PhotoVideo -->
    <owl:Class rdf:about="#PhotoVideo">
        <rdfs:comment>Represents posts containing photos or videos uploaded by the user.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Posts"/>
        <owl:disjointWith rdf:resource="#LiveVideo"/>
        <owl:disjointWith rdf:resource="#CheckIn"/>
    </owl:Class>

    <!-- Sub-class: FeelingActivity -->
    <owl:Class rdf:about="#FeelingActivity">
        <rdfs:comment>Represents posts expressing user emotions or activities.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Posts"/>
    </owl:Class>

    <!-- Sub-class: CheckIn -->
    <owl:Class rdf:about="#CheckIn">
        <rdfs:comment>Represents posts where users check-in to a specific location.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Posts"/>
        <owl:disjointWith rdf:resource="#PhotoVideo"/>
        <owl:disjointWith rdf:resource="#LiveVideo"/>
    </owl:Class>

    <!-- Sub-class: LiveVideo -->
    <owl:Class rdf:about="#LiveVideo">
        <rdfs:comment>Represents live video broadcasts created by the user.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Posts"/>
    </owl:Class>

    <!-- Data Property: hasBackgroundColor -->
    <owl:DatatypeProperty rdf:about="#hasBackgroundColor">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the background color of a post.</rdfs:comment>
        <rdfs:domain rdf:resource="#Posts"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostLocation -->
    <owl:DatatypeProperty rdf:about="#hasPostLocation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores Post's geotag location</rdfs:comment>
        <rdfs:domain rdf:resource="#Posts"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasPostTimestamp -->
    <owl:DatatypeProperty rdf:about="#hasPostTimestamp">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores post's timestamp</rdfs:comment>
        <rdfs:domain rdf:resource="#Posts"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasGIF -->
    <owl:DatatypeProperty rdf:about="#hasGIF">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores the GIF attached to a post, if any.</rdfs:comment>
        <rdfs:domain rdf:resource="#Posts"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Sub-class: LifeEvent -->
    <owl:Class rdf:about="#LifeEvent">
        <rdfs:comment>Represents significant life events shared by the user, such as marriage or graduation.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Posts"/>
    </owl:Class>

    <!-- Class: Stories -->
    <owl:Class rdf:about="#Stories">
        <rdfs:comment>Represents temporary stories shared by users, which expire after a certain period of time.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ContentCreation"/>
        <owl:disjointWith rdf:resource="#Posts"/>
        <owl:disjointWith rdf:resource="#Reels"/>
    </owl:Class>

    <owl:ObjectProperty rdf:about="#postedBy">
        <rdfs:domain rdf:resource="#Stories"/>
        <rdfs:range rdf:resource="#UserProfile"/>
        <rdfs:comment>Indicates the poster of a story.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Sub-class: StoryPhotoVideo -->
    <owl:Class rdf:about="#StoryPhotoVideo">
        <rdfs:comment>Represents a story containing a photo or video.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Stories"/>
    </owl:Class>

    <!-- Sub-class: StoryText -->
    <owl:Class rdf:about="#StoryText">
        <rdfs:comment>Represents a story containing only text.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Stories"/>
    </owl:Class>

    <!-- Data Property: hasStoryMusic -->
    <owl:DatatypeProperty rdf:about="#hasStoryMusic">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores music added to a story.</rdfs:comment>
        <rdfs:domain rdf:resource="#Stories"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Data Property: hasStoryLocation -->
    <owl:DatatypeProperty rdf:about="#hasStoryLocation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Stores story's geotag location</rdfs:comment>
